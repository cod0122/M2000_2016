table(column=(type=long updatewhereclause=yes name=id_alarm_instock dbname="v_alarm_instock_tosend.id_alarm_instock" )
 column=(type=char(32766) updatewhereclause=yes name=email dbname="email" )
 retrieve="   SELECT distinct v1.id_alarm_instock id_alarm_instock
				,  IIF(trim(alarm_instock.email) > ' '
    					 ,trim(alarm_instock.email) + ';'
    					 ,'') 
				+ coalesce((select coalesce(sr_utenti.email, '') 
						from  
						  sr_settori_profili 
                  left outer join sr_prof_utenti on sr_settori_profili.id_sr_profilo = sr_prof_utenti.id_profili
						left outer join sr_utenti on sr_prof_utenti.id_utenti = sr_utenti.id and trim(sr_utenti.email) > ' '
                  where alarm_instock.sr_settore = sr_settori_profili.sr_settore
						for xml path('')), '')
             email
    FROM v_alarm_instock_tosend as v1 inner join alarm_instock on v1.id_alarm_instock = alarm_instock.id_alarm_instock
    order by v1.id_alarm_instock
						
" )

 retrieve="  SELECT v_ptasks_rows.id_ptasks_row,   
         v_ptasks_rows.id_ptask,   
         v_ptasks_rows.id_ptasks_type,   
         v_ptasks_rows.cs_qta,   
         v_ptasks_rows.cs_prezzo,   
         v_ptasks_rows.cs_clienteddtn,   
         v_ptasks_rows.cs_usdqta,   
         v_ptasks_rows.cs_usdprezzo,   
         v_ptasks_rows.cs_pesonettoxlottokg,   
         v_ptasks_rows.cs_custcode,   
         v_ptasks_rows.cs_qtabox,   
         v_ptasks_rows.cs_trackitusn,   
         v_ptasks_rows.cs_trackusitn,   
         v_ptasks_rows.cs_speddata,   
         v_ptasks_rows.cs_charlottedatain,   
         v_ptasks_rows.cs_charlottewo,   
         v_ptasks_rows.cs_charlottewodata,   
         v_ptasks_rows.cs_datarientro,   
         v_ptasks_rows.cs_e1so,   
         v_ptasks_rows.cs_e1sofattura,   
         v_ptasks_rows.cs_e1ancillary,   
         v_ptasks_rows.cs_stgfatturan,   
         v_ptasks_rows.cs_stgfatturadata,   
         v_ptasks_rows.cs_stgfatturaimporto,   
         v_ptasks_rows.cs_stgddtn,   
         v_ptasks_rows.cs_stgddtdata,   
         v_ptasks_rows.cs_docfinen,   
         v_ptasks_rows.cs_notefatt,   
         v_ptasks_rows.cs_invoicen,   
         v_ptasks_rows.cs_invoicedata,   
         v_ptasks_rows.cs_invoicefirmanome,   
         v_ptasks_rows.cs_invoicefirmaruolo,   
         v_ptasks_rows.cs_invoicefirmadata,   
         v_ptasks_rows.x_datins,   
         v_ptasks_rows.x_utente,   
         ptasks_types.descr,   
         ptasks_types.task,
         'vi' k_flag_modalita,
         ptasks.id_meca,
         meca.data_bolla_in,
         0 k_e1so
         ,ptasks.n_ptask
         ,v_ptasks_rows.cs_invoice_id_cliente  
         ,coalesce(trim(clienti.rag_soc_10),'') as invoice_rag_soc_10
         ,v_ptasks_rows.cs_ordine cs_ordine 
    FROM v_ptasks_rows inner join ptasks_types on v_ptasks_rows.id_ptasks_type = ptasks_types.id_ptasks_type 
                       inner join ptasks on v_ptasks_rows.id_ptask = ptasks.id_ptask
                       left outer join meca on ptasks.id_meca = meca.id
                       left outer join clienti on v_ptasks_rows.cs_invoice_id_cliente = clienti.codice
   WHERE ( v_ptasks_rows.id_ptasks_row = :k_id_ptasks_row 
              or (v_ptasks_rows.id_ptask = :k_id_ptask and v_ptasks_rows.id_ptasks_type = :k_id_ptasks_type) )    
" arguments=(("k_id_ptasks_row", number),("k_id_ptask", number),("k_id_ptasks_type", number)) )

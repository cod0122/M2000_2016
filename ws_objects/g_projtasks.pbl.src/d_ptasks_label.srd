 column=(type=long updatewhereclause=yes identity=yes name=cs_qta dbname="cs_qta" )
 column=(type=long updatewhereclause=yes identity=yes name=id_cliente dbname="id_cliente" )
 column=(type=char(61) updatewhereclause=yes name=rag_soc dbname="rag_soc" )
 column=(type=char(4000) updatewhereclause=yes name=validation_proddescr dbname="validation_proddescr" )
 column=(type=char(4000) updatewhereclause=yes name=validation_proddescr_eng dbname="validation_proddescr_eng" )
 column=(type=char(4000) updatewhereclause=yes name=prodlotto dbname="validation_prodlotto" )

 retrieve="  SELECT ptasks.n_ptask n_ptask,   
         ptasks.date_ins date_ins,   
         isnull(v_ptasks_rows.cs_qta, 0) cs_qta,   
         isnull(ptasks.id_cliente, 0) id_cliente,   
         concat(trim(isnull(clienti.rag_soc_10, ''))   
         		,' ', trim(isnull(clienti.rag_soc_11, ''))) rag_soc,   
         trim(isnull(v_ptasks_rows.validation_proddescr, '')) validation_proddescr,   
         trim(isnull(v_ptasks_rows.validation_proddescr_eng, '')) validation_proddescr_eng,   
         trim(isnull(v_ptasks_rows.validation_prodlotto, '')) validation_prodlotto,   
         isnull(v_ptasks_rows.validation_dose_min, 0.0) validation_dose_min,   
         isnull(v_ptasks_rows.validation_dose_max, 0.0) validation_dose_max,   
         trim(isnull(v_ptasks_rows.cs_clienteddtn, '')) cs_clienteddtn 
    FROM ptasks inner join v_ptasks_rows on ptasks.id_ptask = v_ptasks_rows.id_ptask
 					 inner join ptasks_types on v_ptasks_rows.id_ptasks_type = ptasks_types.id_ptasks_type and ptasks_types.task = 'DSV'
                inner join clienti on ptasks.id_cliente = clienti.codice
   WHERE ptasks.id_ptask = :k_id_ptask" arguments=(("k_id_ptask", number)) )

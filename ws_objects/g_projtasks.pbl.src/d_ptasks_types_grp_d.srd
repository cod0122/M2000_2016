 column=(type=long updatewhereclause=yes name=id_ptasks_type dbname="id_ptasks_type" )
 column=(type=long updatewhereclause=yes name=active dbname="active" values="	1/	0" )
 column=(type=char(3) updatewhereclause=yes name=task dbname="task" )
 column=(type=char(160) updatewhereclause=yes name=descrizione dbname="descrizione" )
 retrieve="  SELECT 1 as k_presente
         ,coalesce(j_id_ptasks_type, 0) id_ptasks_type
			,coalesce(pt.active, 0) active
			,coalesce(trim(pt.task), '***') task
			,coalesce(trim(pt.descr), '*rimossa*') descrizione
	 FROM [dbo].[ptasks_types_grp] 
            OUTER APPLY OPENJSON(JSON_QUERY(ptasks_types_json, '$.id_ptasks_type'))
                  WITH (j_id_ptasks_type int '$')
          left outer join  [dbo].[ptasks_types] as pt on j_id_ptasks_type = pt.id_ptasks_type 
   WHERE ptasks_types_grp.id_ptasks_types_grp = :k_id_ptasks_types_grp
           and pt.active = 1    

union all
  SELECT 0 as k_presente
         ,id_ptasks_type
			,coalesce(active, 0) active
			,coalesce(trim(task), '***') task
			,coalesce(trim(descr), '*rimossa*') descrizione
    FROM ptasks_types
   where id_ptasks_type not in
      ( select coalesce(id_ptasks_type, 0)
	        FROM [dbo].[ptasks_types_grp] 
               OUTER APPLY OPENJSON(JSON_QUERY(ptasks_types_json, '$.id_ptasks_type'))
                  WITH (id_ptasks_type int '$')
           where ptasks_types_grp.id_ptasks_types_grp = :k_id_ptasks_types_grp
      )
order by 1 desc, 2

" arguments=(("k_id_ptasks_types_grp", number)) )

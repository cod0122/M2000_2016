 retrieve="  SELECT v_ptasks_rows.id_ptasks_row,   
         v_ptasks_rows.id_ptask,   
         v_ptasks_rows.id_ptasks_type   
         ,(v_ptasks_rows.validation_modaccompn)      validation_modaccompn
         ,trim(v_ptasks_rows.validation_modaccomprogr)      validation_modaccomprogr
         ,(v_ptasks_rows.validation_modaccompdata)      validation_modaccompdata
         ,trim(v_ptasks_rows.validation_modaccompqtadescr)      validation_modaccompqtadescr
         ,trim(v_ptasks_rows.validation_proddescr) 			validation_proddescr 			
         ,trim(v_ptasks_rows.validation_prodlotto) 			validation_prodlotto 			
         ,trim(v_ptasks_rows.validation_laboratorio)		validation_laboratorio			
         ,trim(v_ptasks_rows.validation_qaa)				validation_qaa					
         ,v_ptasks_rows.validation_campioniqta			    validation_campioniqta			
         ,trim(v_ptasks_rows.validation_offertarif)			validation_offertarif			
         ,trim(v_ptasks_rows.validation_attivitacod)			validation_attivitacod			
         ,v_ptasks_rows.validation_speddata				    validation_speddata				
         ,v_ptasks_rows.validation_finepresuntadata		    validation_finepresuntadata	
         ,trim(v_ptasks_rows.validation_laboratoriocertifn)	validation_laboratoriocertifn
         ,v_ptasks_rows.validation_laboratoriocertifdata	validation_laboratoriocertifdata
         ,trim(v_ptasks_rows.validation_laboratoriocertif1n)	validation_laboratoriocertif1n
         ,v_ptasks_rows.validation_laboratoriocertif1data	validation_laboratoriocertif1data
         ,v_ptasks_rows.validation_dose					    validation_dose					
         ,v_ptasks_rows.validation_dose_min				    validation_dose_min				
         ,v_ptasks_rows.validation_dose_max				    validation_dose_max				
         ,trim(v_ptasks_rows.validation_notereport)			validation_notereport	
         ,trim(v_ptasks_rows.validation_noterifpo)			validation_noterifpo
         ,trim(v_ptasks_rows.validation_notealtre)			validation_notealtre
         ,v_ptasks_rows.x_datins
         ,v_ptasks_rows.x_utente   
         ,ptasks_types.descr
         ,ptasks_types.task
         ,'in' k_flag_modalita 
         ,ptasks.n_ptask
         ,trim(v_ptasks_rows.validation_proddescr_eng) 	validation_proddescr_eng
    FROM v_ptasks_rows inner join ptasks_types on v_ptasks_rows.id_ptasks_type = ptasks_types.id_ptasks_type 
                       inner join ptasks on v_ptasks_rows.id_ptask = ptasks.id_ptask
                       left outer join meca on ptasks.id_meca = meca.id
   WHERE ( v_ptasks_rows.id_ptasks_row = :k_id_ptasks_row 
              or (v_ptasks_rows.id_ptask = :k_id_ptask and v_ptasks_rows.id_ptasks_type = :k_id_ptasks_type) )    
" arguments=(("k_id_ptasks_row", number),("k_id_ptask", number),("k_id_ptasks_type", number)) )

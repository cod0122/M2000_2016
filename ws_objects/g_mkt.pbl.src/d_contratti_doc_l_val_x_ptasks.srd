 column=(type=char(65) updatewhereclause=yes name=ptasks_cntr_val dbname="ptasks_cntr_val" )
 retrieve="  SELECT ctr.id_contratto_doc,   
         ctr.anno,   
         ctr.offerta_data,   
         ctr.id_cliente,   
         trim(clienti.rag_soc_10) rag_soc_10,   
         ctr.stato,   
         ctr.offerta_validita,
         ctr.oggetto,   
         ctr.data_inizio,   
         ctr.data_fine,   
          trim(ctr.id_clie_settore) id_clie_settore,   
         ctr.gruppo,   
         ctr.nome_contatto,   
         ctr.x_datins,   
         ctr.x_utente  
	  ,ctr.quotazione_tipo
	  ,ctr.quotazione_cod
	  ,ctr.cliente_desprod_rid
     ,trim(convert(varchar, ctr.quotazione_tipo)) + trim(convert(varchar, ctr.id_contratto_doc)) + '/' + trim(convert(varchar(4), ctr.anno)) as ptasks_cntr_val
    FROM v_contratti_doc ctr 
							LEFT OUTER JOIN clienti ON ctr.id_cliente = clienti.codice 
  WHERE (:k_id_cliente = 0 or ctr.id_cliente = :k_id_cliente) and ctr.quotazione_tipo = 'VAL'
" arguments=(("k_id_cliente", number))  sort="id_cliente A data_fine D id_contratto_doc D " )

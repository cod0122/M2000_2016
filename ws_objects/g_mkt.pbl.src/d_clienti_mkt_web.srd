 retrieve="  SELECT clienti_mkt.id_cliente,   
         trim(clienti_mkt.qualifica) as clienti_mkt_qualifica,   
         clienti_mkt.id_cliente_link,   
         c_link.rag_soc_10,   
         trim(clienti_mkt.altra_sede) as clienti_mkt_altra_sede,   
         trim(clienti_mkt.cod_atecori) as clienti_mkt_cod_atecori,   
         clienti_mkt.id_contatto_1,   
         c1.rag_soc_10,   
         trim(clienti_mkt.contatto_1_qualif) as clienti_mkt_contatto_1_qualif,   
         clienti_mkt.id_contatto_2,   
         c2.rag_soc_10,   
         trim(clienti_mkt.contatto_2_qualif) as clienti_mkt_contatto_2_qualif,   
         clienti_mkt.id_contatto_3,   
         c3.rag_soc_10,   
         trim(clienti_mkt.contatto_3_qualif) as clienti_mkt_contatto_3_qualif,   
         clienti_mkt.id_contatto_4,   
         c4.rag_soc_10,   
         trim(clienti_mkt.contatto_4_qualif) as clienti_mkt_contatto_4_qualif,   
         clienti_mkt.id_contatto_5,   
         c5.rag_soc_10,   
         trim(clienti_mkt.contatto_5_qualif) as clienti_mkt_contatto_5_qualif,   
         trim(clienti_mkt.note_attivita) as clienti_mkt_note_attivita,   
         trim(clienti_mkt.note_prodotti) as clienti_mkt_note_prodotti,   
         clienti_mkt.tipo_rapporto,   
         trim(clienti_web.email) as email,   
         trim(clienti_web.email1) as email1,   
         trim(clienti_web.email2) as email2,   
         trim(clienti_web.note) as note,   
         trim(clienti_web.sito_web) as sito_web,   
         trim(clienti_web.sito_web1) as sito_web1,   
         trim(clienti_web.blog_web) as blog_web,   
         trim(clienti_web.blog_web1) as blog_web1,    
         clienti_mkt.x_datins,   
         clienti_mkt.x_utente,   
         clienti_web.x_datins,   
         clienti_web.x_utente,  
         coalesce(clienti_mkt.gruppo, 0) gruppo,   
         gru.des, 
         clienti_mkt.doc_esporta,   
         trim(clienti_mkt.doc_esporta_prefpath) as doc_esporta_prefpath, 
         trim(clienti_web.email3) as email3,   
         coalesce(clienti_web.email_prontomerce, 0) as email_prontomerce,   
         coalesce(clienti_fatt.email_invio, '0') as email_invio   
         ,coalesce(clienti_web.email_send_certif_off, 0) as email_send_certif_off   
    FROM (clienti left outer join clienti_mkt on  clienti.codice = clienti_mkt.id_cliente
                           left outer join clienti as c1 on clienti_mkt.id_contatto_1 = c1.codice
   						left outer join clienti as c2 on clienti_mkt.id_contatto_2 = c2.codice
   						left outer join clienti as c3 on clienti_mkt.id_contatto_3 = c3.codice
   						left outer join clienti as c4 on clienti_mkt.id_contatto_4 = c4.codice
   						left outer join clienti as c5 on clienti_mkt.id_contatto_5 = c5.codice
   						left outer join clienti as c_link on clienti_mkt.id_cliente_link = c_link.codice
   						left outer join gru on clienti_mkt.gruppo = gru.codice)
                     left outer join clienti_web on  clienti.codice = clienti_web.id_cliente
                     left outer join clienti_fatt on  clienti.codice = clienti_fatt.id_cliente
        			   
   WHERE  clienti.codice = :k_id_cliente
" update="clienti_mkt, clienti_web, clienti_fatt" updatewhere=0 updatekeyinplace=no arguments=(("k_id_cliente", number)) )

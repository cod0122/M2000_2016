 retrieve="  SELECT clienti_mkt.id_cliente,    
         clienti_mkt.qualifica,   
         clienti_mkt.id_cliente_link,   
         clienti_mkt.altra_sede,   
         clienti_mkt.cod_atecori,   
         clienti_mkt.tipo_rapporto,   
         clienti_mkt.note_attivita,   
         clienti_mkt.note_prodotti,   
         clienti_mkt.gruppo,   
         clienti_mkt.doc_esporta,   
         clienti_mkt.doc_esporta_prefpath,   
         gru.des,   
         c_link.rag_soc_10,   
         clienti_mkt.x_datins,   
         clienti_mkt.x_utente    
    FROM clienti_mkt left outer join clienti as c_link on clienti_mkt.id_cliente_link = c_link.codice
   						left outer join gru on clienti_mkt.gruppo = gru.codice
   WHERE  clienti_mkt.id_cliente = :k_id_cliente
" arguments=(("k_id_cliente", number)) )

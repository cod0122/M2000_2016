 retrieve="  SELECT   clienti.codice ,
			clienti.tipo,
			clienti.stato,
			clienti.data_attivazione,
			clienti.id_clie_settore,
			clienti.id_clie_classe,
             trim(clienti.rag_soc_10) as  rag_soc_10,
             trim(clienti.rag_soc_11) as  rag_soc_11,
             trim(clienti.indi_1) as      indi_1,
           clienti.cap_1 ,
             trim(clienti.loc_1) as       loc_1,
           clienti.prov_1 ,
           clienti.zona ,
           clienti.p_iva ,
           clienti.cf,
             clienti.fono as  fono,
             clienti.fax as   fax, 
           clienti.cod_pag, 
           clienti.banca, 
           clienti.abi, 
           clienti.cab, 
           clienti.tipo_banca, 
           clienti.iva , 
           clienti.iva_valida_dal, 
           clienti.iva_valida_al, 
           clienti.iva_esente_imp_max, 
           clienti.iva_esente_imp_min_x_fatt, 
           clienti.mese_es_1, 
           clienti.mese_es_2, 
           trim(clienti.cadenza_fattura) as cadenza_fattura,
             trim(clienti.rag_soc_20) as   rag_soc_20,
             trim(clienti.rag_soc_21) as   rag_soc_21,
            trim(clienti.indi_2) as indi_2,
           clienti.cap_2 ,
             trim(clienti.loc_2) as loc_2,
           clienti.id_nazione_1 ,
           clienti.id_nazione_2 ,
           clienti.id_meca_causale ,
           nazioni.nome ,
           nazioni.area ,
		  trim(  nazioni.nome ) + ~"  ( ~" + trim(  nazioni.area ) + ~" ) ~"  as nazione,  	
		  trim(  nazioni1.nome ) + ~"  ( ~" + trim(  nazioni1.area ) + ~" ) ~"  as nazione1,  	
		  trim(  nazioni2.nome ) + ~"  ( ~" + trim(  nazioni2.area ) + ~" ) ~"  as nazione2,  	
           clienti.prov_2 ,
           clienti.x_datins,
           clienti.x_utente,
             trim(ind_comm.rag_soc_1_c) as  rag_soc_1_c,
             trim(ind_comm.rag_soc_2_c) as  rag_soc_2_c,
             trim(ind_comm.indi_c) as       indi_c,
            ind_comm.cap_c as ind_comm_cap_c,
             trim(ind_comm.loc_c) as        loc_c,
            ind_comm.prov_c as ind_comm_prov_c,
            ind_comm.clie_c  as ind_comm_clie_c, 
           ind_comm.id_nazione_c ,
			pagam.des as pagam_des,
			iva.des as iva_des
           ,clienti_fatt.fattura_da
           ,clienti_fatt.note_1
           ,clienti_fatt.note_2
           ,clienti_fatt.modo_stampa
           ,clienti_fatt.modo_email
           ,clienti_fatt.email_invio
		,clie_settori.descr
		,clie_classi.descr
		,meca_causali.descrizione  
           ,COALESCE(trim(clienti_fatt.codice_ipa), '') as codice_ipa
           ,COALESCE(clienti_fatt.impon_minimo, 0) as impon_minimo
		  ,COALESCE(clienti.e1an, 0) as e1an
		  ,trim(COALESCE(clienti.e1ancodrs, '')) as e1ancodrs
		  ,trim(COALESCE(clienti.pklbcodepref, '')) as pklbcodepref
          ,coalesce(clienti.delivery_dd_after, 0) delivery_dd_after
          ,coalesce(clienti.delivery_hour,CAST('00:00' AS TIME)) delivery_hour
        ,COALESCE((select max(clienti_memo.id_memo) from clienti_memo where clienti.codice = clienti_memo.id_cliente),0) as id_memo
        ,coalesce(clienti_altro.e1_asn_ehvr01, 0) e1_asn_ehvr01
        FROM clienti left outer join ind_comm on clienti.codice = ind_comm.clie_c  
                           left outer join nazioni on clienti.id_nazione_1 = nazioni.id_nazione    
                           left outer join nazioni as nazioni1 on clienti.id_nazione_2 = nazioni1.id_nazione   
                           left outer join nazioni as nazioni2 on ind_comm.id_nazione_c = nazioni2.id_nazione 
                           left outer join pagam on clienti.cod_pag = pagam.codice
                           left outer join iva on clienti.iva = iva.codice
                           left outer join clienti_fatt on clienti.codice = clienti_fatt.id_cliente 
                           left outer join clie_settori on clienti.id_clie_settore = clie_settori.id_clie_settore 
                           left outer join clie_classi on clienti.id_clie_classe = clie_classi.id_clie_classe 
         				      left outer join meca_causali on clienti.id_meca_causale = meca_causali.id_meca_causale
        				         left outer join clienti_altro on clienti.codice = clienti_altro.id_cliente
        WHERE ( clienti.codice = :k_id_cliente )   
" arguments=(("k_id_cliente", number)) )

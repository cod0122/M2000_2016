 retrieve="  SELECT distinct clienti.codice as id_cliente,   
         concat(trim(coalesce(clienti.rag_soc_10,'')), ' ', trim(coalesce(clienti.rag_soc_11,''))) as rag_soc_1,   
         trim(coalesce(clienti.loc_1,'')) as localita,   
         clienti.prov_1 as prov,
         trim(coalesce(clienti.id_nazione_1,'')) as id_nazione_1
       ,trim(coalesce(nazioni.nome,'')) nazioni_nome
       ,trim(clienti.stato) stato
       ,trim(coalesce(clienti.tipo,'')) tipo
    ,trim(coalesce(clienti.indi_1,'')) indi_1
    ,concat(trim(coalesce(clienti_contatti.rag_soc_10,'')), ' ', trim(coalesce(clienti_contatti.rag_soc_11,''))) clienti_contatti_rag_soc 
    FROM clienti  
               left outer join nazioni on clienti.id_nazione_1 = nazioni.id_nazione
               left outer join clienti_mkt as clienti_mkt_invoice on clienti.codice = clienti_mkt_invoice.id_cliente
               left outer join clienti as clienti_contatti on clienti_mkt_invoice.id_contatto_1 = clienti_contatti.codice
   WHERE clienti.tipo = 'P' and( :k_id_cliente = 0 or clienti.codice = :k_id_cliente)
ORDER BY rag_soc_1 ASC   
" arguments=(("k_id_cliente", number)) )

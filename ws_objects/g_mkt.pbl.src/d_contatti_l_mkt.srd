 column=(type=char(12) updatewhereclause=yes name=e1mcu dbname="e1mcu" )
 column=(type=char(8) updatewhereclause=yes name=minerbio dbname="minerbio" )
 column=(type=char(1) updatewhereclause=yes name=for_price_cntct dbname="for_price_cntct" values="	S/	N" )
 column=(type=long updatewhereclause=yes name=k_listino_invigore dbname="k_listino_invigore" values="	1/	0" )
 column=(type=char(120) updatewhereclause=yes name=categ dbname="categ" )
 column=(type=char(30) updatewhereclause=yes name=cognome_contatto dbname="cognome_contatto" )
 column=(type=char(80) updatewhereclause=yes name=cell dbname="cell" )
 retrieve=" SELECT  clienti.codice as id_cliente,   
         COALESCE(c1.e1an,0) as e1an,
        isnull((select trim(descr) from clienti_stato where COALESCE(clienti.stato, '6') = clienti_stato.stato),'') as contatti_stato,
         trim(clienti.rag_soc_10) as nome_contatto,  
         COALESCE(trim(clienti.loc_1),'') as localita,  
         COALESCE(clienti.prov_1,'') as prov,
         COALESCE(clienti.id_nazione_1,'') as id_nazione_1
         ,trim(c1.id_clie_classe) as id_clie_classe
         ,COALESCE((select trim(nazioni.nome) from nazioni where clienti.id_nazione_1 = nazioni.id_nazione),'') as nazioni_nome
        ,COALESCE(trim(clienti.fono),'') as fono
        ,COALESCE(clienti_mkt.gruppo,0) as gruppo
         ,COALESCE((select trim(gru.des) from gru where clienti_mkt.gruppo = gru.codice),'') as gruppo_descrizione
        ,COALESCE((select trim(province.regione) from province where clienti.prov_1 = province.sigla),'') as regione
        ,COALESCE(c1.codice,0) as cod_cli
        ,COALESCE(trim(c1.rag_soc_10),'') as rag_soc_10
        ,case when c1.codice > 0 then
            isnull((select trim(descr) from clienti_stato where COALESCE(c1.stato, '6') = clienti_stato.stato),'')
             else 'Assente'
		   end as stato
        ,COALESCE(trim(clienti_mkt.qualifica),'') as qualifica
        ,concat(v_contatti.email, '  ',  v_contatti.email1, '  ', v_contatti.email2) as web_email_contatto
        ,trim(COALESCE(c1.id_clie_settore,'')) as id_clie_settore
        ,v_contatti.for_qa_italy for_qa_italy
        ,v_contatti.for_cobalt_rload for_cobalt_rload
        ,trim(base.e1mcu) e1mcu
        ,'Minerbio' minerbio
        ,v_contatti.for_price_cntct for_price_cntct
		  ,case when c1.codice > 0 and (select distinct 1 from listino LEFT OUTER JOIN contratti ON listino.contratto = contratti.codice 
		                                where c1.codice = listino.cod_cli
             			and (listino.dt_end >= getdate() or ((listino.dt_end is null or listino.dt_end < '01.01.1990') and contratti.data_scad >= getdate()) )
							and listino.attivo = 'S') > 0 then 1
          else 0 end  as k_listino_invigore
        ,v_contatti.categ categ
        ,trim(clienti.rag_soc_11) as cognome_contatto
        ,v_contatti.cell as cell
    FROM clienti   
              inner join clienti_mkt on (clienti_mkt.id_cliente = clienti.codice)
              left outer join clienti as c1 on clienti_mkt.id_cliente_link = c1.codice
              left outer join v_contatti on clienti.codice = v_contatti.id_cliente 
              ,base
   WHERE clienti.tipo = 'C'
  ORDER BY c1.rag_soc_10 ASC, 3, 4   
" )

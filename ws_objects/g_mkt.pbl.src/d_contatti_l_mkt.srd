 column=(type=char(32) updatewhereclause=yes name=regione dbname="regione" )
 column=(type=long updatewhereclause=yes name=id_cliente dbname="cod_cli" )
 column=(type=char(30) updatewhereclause=yes name=rag_soc_10 dbname="rag_soc_10" )
 column=(type=char(32) updatewhereclause=yes name=stato dbname="stato" )
 column=(type=char(80) updatewhereclause=yes name=qualifica dbname="qualifica" )
 column=(type=char(1540) updatewhereclause=yes name=web_email_contatto dbname="web_email_contatto" )

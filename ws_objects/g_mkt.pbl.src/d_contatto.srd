 retrieve="  SELECT   clienti.codice ,
			 clienti.tipo, 
             trim(clienti.rag_soc_10) as  rag_soc_10,
             trim(clienti.rag_soc_11) as  rag_soc_11,
             trim(clienti.indi_1) as      indi_1,
           clienti.cap_1 ,
             trim(clienti.loc_1) as       loc_1,
           clienti.prov_1 ,
           clienti.zona ,
           clienti.p_iva ,
           clienti.cf,
             trim(clienti.fono) as        fono,
             trim(clienti.fax) as         fax, 
           clienti.id_nazione_1 ,
           nazioni.nome ,
           nazioni.area ,
           clienti.x_datins,
           clienti.x_utente,
         trim(clienti_web.email) as email,   
         trim(clienti_web.email1) as email1,   
         trim(clienti_web.email2) as email2,   
         trim(clienti_web.note) as note,   
         trim(clienti_web.sito_web) as sito_web,   
         trim(clienti_web.sito_web1) as sito_web1,   
         trim(clienti_web.blog_web) as blog_web,   
         trim(clienti_web.blog_web1) as blog_web1   
        FROM clienti  left outer join nazioni on   clienti.id_nazione_1 = nazioni.id_nazione     
                             left outer join clienti_web on   clienti.codice = clienti_web.id_cliente 
        WHERE ( clienti.codice = :k_id_cliente )   
" update="clienti" updatewhere=0 updatekeyinplace=no arguments=(("k_id_cliente", number)) )

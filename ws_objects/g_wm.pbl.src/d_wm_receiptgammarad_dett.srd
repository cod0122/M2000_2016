 retrieve="  SELECT 
         receiptgammarad.customeritem,   
         receiptgammarad.customerlot,   
			(receiptgammarad.quantitasacchi * receiptgammarad.palletquantity) kpz_plt,
         receiptgammarad.quantitasacchi,
         receiptgammarad.packinglistcode,   
         receiptgammarad.palletquantity,   
         receiptgammarad.pesonetto,
         receiptgammarad.pesolordo,
         receiptgammarad.externalpalletcode,
         trim(receiptgammarad.barcodewo) barcodewo
    FROM receiptgammarad  
   WHERE receiptgammarad.idwmpklist = :k_idwmpklist
ORDER BY receiptgammarad.customeritem ASC  
         ,externalpalletcode asc 

" arguments=(("k_idwmpklist", number)) )

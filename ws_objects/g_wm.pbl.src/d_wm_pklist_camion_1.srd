 retrieve="  SELECT wm_pklist_camion.id_wm_pklist_camion,   
         wm_pklist_camion.idpkl,   
         wm_pklist_camion.dataarrivo,   
         wm_pklist_camion.seqcamion,   
         wm_pklist_camion.totpallet,   
         wm_pklist_camion.id_cliente,
         isnull(wm_pklist.clie_3, 0) clie_3,   
         isnull(clienti.rag_soc_10,'') rag_soc_10,   
         wm_pklist.idpkl wm_pklist_id_pkl,
         isnull(meca.id, 0) id_meca,
         isnull(meca.num_int, 0) num_int,   
         meca.data_int data_int,   
         meca.data_ent data_ent,   
         isnull(meca.clie_1, 0) clie_1,   
         isnull(c1.rag_soc_10,'') c1_rag_soc_10,   
         isnull(meca.clie_2, 0) clie_2,   
         isnull(c2.rag_soc_10,'') c2_rag_soc_10,   
         wm_pklist_camion.x_utente,   
         wm_pklist_camion.x_datins  
    FROM wm_pklist_camion 
                         LEFT OUTER JOIN wm_pklist ON wm_pklist_camion.idpkl = wm_pklist.idpkl_in   
								 LEFT OUTER JOIN clienti ON wm_pklist.clie_3 = clienti.codice 
								 LEFT OUTER JOIN meca ON wm_pklist.id_wm_pklist = meca.id_wm_pklist 
								 LEFT OUTER JOIN clienti c1 ON meca.clie_1 = c1.codice 
								 LEFT OUTER JOIN clienti c2 ON meca.clie_2 = c2.codice 
        where wm_pklist_camion.id_wm_pklist_camion = :id_wm_pklist_camion" arguments=(("id_wm_pklist_camion", number)) )

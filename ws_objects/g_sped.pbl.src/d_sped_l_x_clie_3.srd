 column=(type=date updatewhereclause=yes name=data_bolla_out dbname="sped.data_bolla_out" dbalias=".data_bolla_out" )
 column=(type=long updatewhereclause=yes identity=yes name=id_sped dbname="sped.id_sped" dbalias=".id_sped" )
 column=(type=long updatewhereclause=yes name=clie_3 dbname="sped.clie_3" dbalias=".clie_3" )
 column=(type=long updatewhereclause=yes name=clie_2 dbname="sped.clie_2" dbalias=".clie_2" )
 column=(type=char(30) updatewhereclause=yes name=clienti_rag_soc_10 dbname="clienti.rag_soc_10" dbalias=".rag_soc_10" )
 column=(type=char(30) updatewhereclause=yes name=clienti_loc_1 dbname="clienti.loc_1" dbalias=".loc_1" )
 column=(type=char(2) updatewhereclause=yes name=clienti_prov_1 dbname="clienti.prov_1" dbalias=".prov_1" )
 column=(type=char(3) updatewhereclause=yes name=id_nazione_1 dbname="clienti.id_nazione_1" dbalias=".id_nazione_1" )
 column=(type=char(4000) updatewhereclause=yes name=causale dbname="sped.causale" dbalias=".causale" )
 column=(type=char(4000) updatewhereclause=yes name=cura_trasp dbname="sped.cura_trasp" dbalias=".cura_trasp" values="	V/	0" )
 column=(type=date updatewhereclause=yes name=k_ritiro_data dbname="sped.k_ritiro_data" dbalias=".k_ritiro_data" )
 column=(type=char(8) updatewhereclause=yes name=k_ritiro_ora dbname="k_ritiro_ora" )
 retrieve="  SELECT
         sped.num_bolla_out ,   
         sped.data_bolla_out,   
         sped.id_sped,   
         sped.clie_3 ,   
         sped.clie_2 ,   
         clienti.rag_soc_10,   
         clienti.loc_1,   
         clienti.prov_1,   
         clienti.id_nazione_1,   
         sped.causale  
        ,sped.cura_trasp 
        ,sped.data_rit as k_ritiro_data
        ,(COALESCE(sped.ora_rit,'')) as k_ritiro_ora
  FROM sped LEFT OUTER JOIN clienti ON sped.clie_2 = clienti.codice
  where sped.data_bolla_out > DATEADD(DAY, -300, getdate())   
        and (:k_clie_3 = 0 or sped.clie_3 = :k_clie_3)
union all
  SELECT
         convert(int, SUBSTRING(sped_free.num_bolla_out, PATINDEX('%[0-9]%', sped_free.num_bolla_out)
, PATINDEX('%[0-9][^0-9]%', sped_free.num_bolla_out + 't') - PATINDEX('%[0-9]%', 
                    sped_free.num_bolla_out) + 1)) ,   
         sped_free.data_bolla_out,   
         sped_free.id_sped_free,   
         sped_free.clie_3 ,   
         sped_free.clie_2 ,   
         clienti.rag_soc_10,   
         clienti.loc_1,   
         clienti.prov_1,   
         clienti.id_nazione_1,   
         sped_free.causale  
        ,sped_free.trasporto
        , case 
           when sped_free.data_ora_rit > ' ' then
               convert (date, 
(SUBSTRING(sped_free.data_ora_rit, 5,4) + '-'
                 + SUBSTRING(sped_free.data_ora_rit, 3,2) + '-'
                     + SUBSTRING(sped_free.data_ora_rit, 1,2)))
            else null
          end
        , case 
           when sped_free.data_ora_rit > ' ' then
               (SUBSTRING(sped_free.data_ora_rit, 9,2) + ':'
                 + SUBSTRING(sped_free.data_ora_rit, 11,2))
            else null
          end
  FROM v_sped_free sped_free LEFT OUTER JOIN clienti ON sped_free.clie_2 = clienti.codice
  where sped_free.data_bolla_out > DATEADD(DAY, -300, getdate())   
        and (:k_clie_3 = 0 or sped_free.clie_3 = :k_clie_3)




 column=(type=char(1) updatewhereclause=yes name=sped_stampa dbname="sped.stampa" dbalias=".stampa" values="Da Stampare	N/Stampato (da fatturare)	S/Fatturato	F/da Stampare	/" )
 column=(type=char(255) updatewhereclause=yes name=sped_rag_soc_1 dbname="sped.rag_soc_1" dbalias=".rag_soc_1" )
 column=(type=char(255) updatewhereclause=yes name=sped_rag_soc_2 dbname="sped.rag_soc_2" dbalias=".rag_soc_2" )

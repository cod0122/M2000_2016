table(column=(type=long updatewhereclause=yes name=id_sped dbname="sped.id_sped" dbalias=".id_sped" )
 column=(type=long updatewhereclause=yes name=num_bolla_out dbname="sped.num_bolla_out" dbalias=".num_bolla_out" )

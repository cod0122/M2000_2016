 column=(type=char(5) updatewhereclause=yes name=sped_cap dbname="sped.cap" dbalias=".cap" )
 column=(type=char(255) updatewhereclause=yes name=sped_loc dbname="sped.loc" dbalias=".loc" )
 column=(type=char(3) updatewhereclause=yes name=sped_id_nazione dbname="sped.id_nazione" dbalias=".id_nazione" )
 column=(type=decimal(0) updatewhereclause=yes name=colli_2 dbname="colli_2" )
 column=(type=decimal(0) updatewhereclause=yes name=colli dbname="colli" )
 column=(type=decimal(0) updatewhereclause=yes name=colli_out dbname="colli_out" )
 column=(type=char(4) updatewhereclause=yes name=deposito_sigla dbname="deposito_sigla" )
 column=(type=char(80) updatewhereclause=yes name=deposito_descr dbname="deposito_descr" )
 column=(type=char(2) updatewhereclause=yes name=numpref_bolla_out dbname="numpref_bolla_out" )
 retrieve="  SELECT  
         sped.id_sped id_sped,
         sped.num_bolla_out as num_bolla_out,   
         sped.data_bolla_out,   
         sped.clie_2,   
         sped.clie_3,   
         clienti.rag_soc_10,   
         c3.rag_soc_10 as clienti_fatturato,   
         sped.data_rit,   
         sped.ora_rit,   
		sped.stampa,
		sped.rag_soc_1,
		sped.rag_soc_2,
		sped.indi,
		sped.cap,
		sped.loc,
		sped.id_nazione,
         sum(armo.colli_2) as colli_2,   
         sum(arsp.colli) as colli,     
        sum(arsp.colli_out) as colli_out
      ,trim(isnull(depositi.sigla, '')) as deposito_sigla
      ,trim(isnull(depositi.descr, '')) as deposito_descr
      ,isnull(sped.numpref_bolla_out,'') as numpref_bolla_out
    FROM sped  inner JOIN arsp  ON sped.id_sped = arsp.id_sped
             	inner JOIN armo  ON arsp.id_armo = armo.id_armo
               LEFT OUTER JOIN clienti ON sped.clie_2 = clienti.codice
               LEFT OUTER JOIN clienti as c3 ON sped.clie_3 = c3.codice
					left outer join depositi on sped.id_deposito = depositi.id_deposito
   WHERE 
		 sped.id_sped = :k_id_sped
group by 
         sped.id_sped,
			sped.numpref_bolla_out,
         sped.num_bolla_out,   
         sped.data_bolla_out,   
         sped.clie_2,   
         sped.clie_3,   
         clienti.rag_soc_10,   
         sped.data_rit,   
         sped.ora_rit,   
		c3.rag_soc_10,
		sped.stampa,
		sped.rag_soc_1,
		sped.rag_soc_2,
		sped.indi,
		sped.cap,
		sped.loc,
		sped.id_nazione
     ,depositi.sigla
     ,depositi.descr
	  ,sped.numpref_bolla_out
;
" arguments=(("k_id_sped", number)) )

 retrieve="  SELECT distinct
         sr_settori_profili.id_sr_settore_profilo,
         sr_settori_profili.sr_settore,
         sr_utenti.id,   
         sr_utenti.codice,   
         sr_utenti.nome,   
         sr_profili.id,   
         sr_profili.nome,
         sr_profili.descrizione,
         sr_utenti.stato,
         sr_profili.stato
    FROM sr_settori_profili inner join sr_profili on sr_settori_profili.id_sr_profilo = sr_profili.id
                                inner JOIN sr_prof_utenti ON sr_prof_utenti.id_profili = sr_profili.id  
                                inner join sr_prof_funz on  sr_prof_utenti.id_profili = sr_prof_funz.id_profili
                               inner join sr_utenti on  sr_prof_utenti.id_utenti = sr_utenti.id  
          
   WHERE   
         ( ( sr_settori_profili.sr_settore = :k_id ) )   
ORDER BY sr_utenti.codice ASC   
" arguments=(("k_id", string)) )

 retrieve="  SELECT pl_barcode.codice,   
         pl_barcode.data,   
         isnull(trim(pl_barcode.note_1), '') note_1,   
         isnull(trim(pl_barcode.note_2), '') note_2,   
         pl_barcode.data_chiuso,   
         pl_barcode.data_sosp,   
         pl_barcode.x_utente,   
         pl_barcode.x_datins,   
         pl_barcode.groupage,   
         pl_barcode.priorita,   
         pl_barcode.prima_del_barcode,   
         pl_barcode.upd_data_chiuso,   
         pl_barcode.upd_utente_chiuso  
         ,case
           when pl_barcode.stato_pl > ' ' then trim(pl_barcode.stato_pl)
           else '' 
          end stato_pl
         , isnull(pl_barcode.id_programma, 0) id_programma
    FROM pl_barcode  
   WHERE ( pl_barcode.codice = :k_codice )    
" update="pl_barcode" updatewhere=0 updatekeyinplace=no arguments=(("k_codice", number)) )

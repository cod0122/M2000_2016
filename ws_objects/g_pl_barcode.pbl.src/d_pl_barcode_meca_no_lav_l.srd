 retrieve="SELECT 
         sum(contati)            as contati,
         sum(contati_orig)       as contati_orig,
         v_meca_pl.data_int      as data_int,   
         v_meca_pl.num_int       as num_int,   
		   (armo.colli_2)          as armo_colli_2,
         v_meca_pl.contratto  as contratto,   
         upper(area_mag)         as meca_area_mag,    
         v_meca_pl.mc_co     as mc_co,
         isnull(v_meca_pl.sc_cf, '') as sc_cf,   
         v_meca_pl.sl_pt      as sl_pt,   
         v_meca_pl.descr      as descr,   
         v_meca_pl.clie_1     as clie_1,   
         (trim(c1.rag_soc_10) + '  (' + convert(varchar,clie_1) + ')' ) as k_mandante ,    
         v_meca_pl.clie_2     as clie_2,   
         (trim(c2.rag_soc_10) + '  (' + convert(varchar,clie_2) + ')' ) as k_ricevente ,    
         v_meca_pl.clie_3     as clie_3,   
         (trim(c3.rag_soc_10) + '  (' + convert(varchar,clie_3) + ')' ) as k_fatturato ,    
         v_meca_pl.num_bolla_in  as num_bolla_in,   
         v_meca_pl.data_bolla_in as data_bolla_in,  
         v_meca_pl.consegna_dataora as consegna_dataora,
         v_meca_pl.fila_1    as fila_1,   
         v_meca_pl.fila_2    as fila_2,   
         v_meca_pl.fila_1p   as fila_1p,   
         v_meca_pl.fila_2p   as fila_2p,   
         sum(armo.pedane / armo.colli_2 * v_meca_pl.contati)  as pedane,
         armo.dose              as dose, 
         sum(armo.peso_kg / armo.colli_2 * v_meca_pl.contati) as peso_kg, 
         isnull(v_meca_pl.grp,'') as grp
         ,v_meca_pl.stato_in_attenzione as stato_in_attenzione
         ,v_meca_pl.id       as id
         ,COALESCE(trim(v_meca_dosim_barcode_max.barcode_dosimetro), '') as barcode_dosimetro
         ,v_meca_pl.k_wm_associato     as k_wm_associato 
         ,armo.note_2       as note_2
	    	,area_mag_pos
		   ,0 as evidenzia_area_mag_value
        ,CASE 
             WHEN sl_pt.densitamax > 0 then sl_pt.densitamax
               ELSE isnull(sl_pt.densita, 0.00) 
         END as k_densita
       ,(armo.alt_2 / 1000 * armo.lung_2 / 1000 * armo.larg_2 / 1000) as dim_m3  
       ,v_meca_pl.e1doco    as e1doco
       ,v_meca_pl.e1rorn    as e1rorn
       ,v_meca_pl.data_ent     as data_ent
       ,trim(v_meca_pl.e1srst)  as e1srst
		 ,2 as k_choose_1
		 ,2 as k_choose_2
		 ,9999 as k_nchoosed
       ,CASE
          when (armo.colli_2 - sum(contati))  > 0 then armo.colli_2 
           else 0
         end as armo_colli_contati 
       ,coalesce((select max(asdrackbarcode.id_asdrackcode) from barcode left outer join asdrackbarcode on
                    barcode.barcode = asdrackbarcode.barcode where barcode.id_meca = armo.id_meca), 0)  id_asdrackcode
       ,coalesce((select max(asdslpt.id_asddevice) from asdslpt where asdslpt.cod_sl_pt = v_meca_pl.sl_pt), 0)  id_asddevice
       ,v_meca_pl.impianto
FROM  v_meca_pl_v1 as v_meca_pl inner join armo on v_meca_pl.id_armo = armo.id_armo
         inner join clienti as c1 on v_meca_pl.clie_1 = c1.codice
         inner join clienti as c2 on v_meca_pl.clie_2 = c2.codice
         inner join clienti as c3 on v_meca_pl.clie_3 = c3.codice
         inner join v_meca_dosim_barcode_max  on v_meca_dosim_barcode_max.id_meca = armo.id_meca
         inner JOIN sl_pt         on v_meca_pl.sl_pt = sl_pt.cod_sl_pt
   WHERE
        v_meca_pl.data_int >= :k_data_int  
		  and (v_meca_pl.pl_barcode = 0  
			    or v_meca_pl.pl_barcode = :k_pl_barcode)
	     and armo.magazzino = 2  
		  and armo.colli_2 > 0
group by 
         v_meca_pl.data_int,   
         v_meca_pl.num_int,   
		 	armo.colli_2,
         v_meca_pl.contratto,   
         v_meca_pl.area_mag,    
         v_meca_pl.mc_co,   
         v_meca_pl.sc_cf,   
         v_meca_pl.sl_pt,   
         v_meca_pl.descr,   
         v_meca_pl.clie_1,   
         c1.rag_soc_10,   
         v_meca_pl.clie_2,   
         c2.rag_soc_10,   
         v_meca_pl.clie_3,   
         c3.rag_soc_10,   
         v_meca_pl.num_bolla_in,   
         v_meca_pl.data_bolla_in,  
         v_meca_pl.consegna_dataora,
         v_meca_pl.fila_1,   
         v_meca_pl.fila_2,   
         v_meca_pl.fila_1p,   
         v_meca_pl.fila_2p,   
         v_meca_pl.grp,
         armo.dose 
         ,v_meca_pl.stato_in_attenzione
         ,v_meca_pl.id
         ,v_meca_dosim_barcode_max.barcode_dosimetro
         ,v_meca_pl.k_wm_associato
         ,note_2 
         ,note_2
		,area_mag_pos
		,armo.peso_kg
		,(armo.alt_2 / 1000 * armo.lung_2 / 1000 * armo.larg_2 / 1000)
       ,v_meca_pl.e1doco
       ,v_meca_pl.e1rorn
       ,v_meca_pl.data_ent 
       ,v_meca_pl.e1srst
       ,armo.id_meca
	   ,sl_pt.densitamax
	   ,sl_pt.densita
      ,v_meca_pl.impianto
" update="_meca" updatewhere=0 updatekeyinplace=no arguments=(("k_data_int", date),("k_pl_barcode", number))  sort="k_fatturato A area_mag_pos D meca_num_int A " )

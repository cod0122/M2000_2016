 retrieve="  SELECT trim(dosimetrie.lotto_dosim) as lotto_dosim, 
         dosimetrie.attivo,   
         max(dosimetrie.data) as data,   
         max(trim(dosimetrie.note)) as note,   
         max(dosimetrie.x_datins) as x_datins,   
         max(dosimetrie.x_utente) as x_utente,   
         max(COALESCE(dosimetrie.dosim_tipo, '')) as dosim_tipo  
    FROM dosimetrie  
    where :k_disattivi = 1 
				or dosimetrie.id >= 
             (select min(dosimetrie.id) from dosimetrie where dosimetrie.attivo = 'S')
    group by trim(dosimetrie.lotto_dosim), 
         dosimetrie.attivo

ORDER BY 3 desc   
" arguments=(("k_disattivi", number)) update="dosimetrie" updatewhere=0 updatekeyinplace=no )

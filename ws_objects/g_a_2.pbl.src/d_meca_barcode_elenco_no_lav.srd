 retrieve="SELECT 
         sum(contati) as contati,
         sum(contati_orig) as contati_orig,
         v_meca_pl.data_int as data_int,   
         v_meca_pl.num_int as num_int,   
		   (armo.colli_2) as armo_colli_2,
         contratto,   
         upper(area_mag) as meca_area_mag,    
         mc_co,   
         sc_cf,   
         sl_pt,   
         descr,   
         clie_1,   
         (trim(c1.rag_soc_10) + '  (' + convert(varchar,clie_1) + ')' ) as k_mandante ,    
         clie_2,   
         (trim(c2.rag_soc_10) + '  (' + convert(varchar,clie_2) + ')' ) as k_ricevente ,    
         clie_3,   
         (trim(c3.rag_soc_10) + '  (' + convert(varchar,clie_3) + ')' ) as k_fatturato ,    
         num_bolla_in,   
         data_bolla_in,  
         consegna_dataora,
         fila_1,   
         fila_2,   
         fila_1p,   
         fila_2p,   
         sum(armo.pedane / armo.colli_2 * v_meca_pl.contati)  as pedane,
         armo.dose as dose, 
         sum(armo.peso_kg / armo.colli_2 * v_meca_pl.contati) as peso_kg, 
         max(grp) as grp
         ,stato_in_attenzione
         ,id
         ,COALESCE(trim(v_meca_dosim_barcode_max.barcode_dosimetro), '') as barcode_dosimetro
         ,k_wm_associato
         ,armo.note_2 as note_2
		,area_mag_pos
		,0 as evidenzia_area_mag_value
--      , COALESCE((peso_kg / armo_colli_2 / dim_m3) / 1000, 0.00) as k_densita
--      , densita 
        ,CASE 
             WHEN (armo.alt_2 / 1000 * armo.lung_2 / 1000 * armo.larg_2 / 1000) > 0
               THEN cast((COALESCE((armo.peso_kg / armo.colli_2 / (armo.alt_2 / 1000 * armo.lung_2 / 1000 * armo.larg_2 / 1000)) / 1000, 0.00)) as decimal(6,3))
               ELSE 0 
       END as k_densita
--		 , COALESCE(dim_m3, 0.00) as dim_m3
       ,(armo.alt_2 / 1000 * armo.lung_2 / 1000 * armo.larg_2 / 1000) as dim_m3  
       ,e1doco
       ,e1rorn
       ,v_meca_pl.data_ent as data_ent
       ,trim(e1srst) as e1srst
		 ,2 as k_choose_1
		 ,2 as k_choose_2
		 ,9999 as k_nchoosed
       ,CASE
          when (armo.colli_2 - sum(contati))  > 0 then armo.colli_2 
           else 0
         end as armo_colli_contati 
       ,coalesce((select max(asdrackbarcode.id_asdrackcode) from barcode left outer join asdrackbarcode on
                    barcode.barcode = asdrackbarcode.barcode where barcode.id_meca = armo.id_meca), 0)  id_asdrackcode
       ,coalesce((select max(asdslpt.id_asddevice) from asdslpt where asdslpt.cod_sl_pt = v_meca_pl.sl_pt), 0)  id_asddevice
       ,(select case
                when trim(sl_pt.densitamax) > ' ' then
                    trim(sl_pt.densitamax)
                when trim(sl_pt.densita) > ' ' then
                    trim(sl_pt.densita)
                else ''
             end from sl_pt where sl_pt.cod_sl_pt = v_meca_pl.sl_pt) as densita_pt 
FROM  v_meca_pl_v1 as v_meca_pl inner join armo on v_meca_pl.id_armo = armo.id_armo
         inner join clienti as c1 on v_meca_pl.clie_1 = c1.codice
         inner join clienti as c2 on v_meca_pl.clie_2 = c2.codice
         inner join clienti as c3 on v_meca_pl.clie_3 = c3.codice
         inner join v_meca_dosim_barcode_max  on v_meca_dosim_barcode_max.id_meca = armo.id_meca
	---	inner join meca_dosim on meca_dosim.id_meca = armo.id_meca
   WHERE 
        v_meca_pl.data_int >= :k_data_int  
		  and (v_meca_pl.pl_barcode = 0  
			    or v_meca_pl.pl_barcode = :k_pl_barcode)
	     and armo.magazzino = 2  
		and armo.colli_2 > 0
group by 
         v_meca_pl.data_int,   
         v_meca_pl.num_int,   
			armo.colli_2,
         contratto,   
         area_mag,    
         mc_co,   
         sc_cf,   
         sl_pt,   
         descr,   
         clie_1,   
         c1.rag_soc_10,   
         clie_2,   
         c2.rag_soc_10,   
         clie_3,   
         c3.rag_soc_10,   
         num_bolla_in,   
         data_bolla_in,  
         consegna_dataora,
         fila_1,   
         fila_2,   
         fila_1p,   
         fila_2p,   
         dose 
         ,stato_in_attenzione
         ,id
         ,v_meca_dosim_barcode_max.barcode_dosimetro
         ,k_wm_associato
         ,note_2 
         ,note_2
		,area_mag_pos
--		,evidenzia_area_mag_value
		,armo.peso_kg
		,(armo.alt_2 / 1000 * armo.lung_2 / 1000 * armo.larg_2 / 1000)
--		,36
--		,37
       ,e1doco
       ,e1rorn
       ,data_ent 
       ,e1srst
       ,armo.id_meca
" update="v_meca_pl_v1" updatewhere=0 updatekeyinplace=no arguments=(("k_data_int", date),("k_pl_barcode", number))  sort="k_fatturato A area_mag_pos D meca_num_int A " )

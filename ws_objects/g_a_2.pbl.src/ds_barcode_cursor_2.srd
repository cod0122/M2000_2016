table(column=(type=long updatewhereclause=yes name=id_meca dbname="barcode.id_meca" dbalias=".id_meca" )
 column=(type=date updatewhereclause=yes name=data_int dbname="barcode.data_int" dbalias=".data_int" )
 column=(type=long updatewhereclause=yes name=num_int dbname="barcode.num_int" dbalias=".num_int" )
 column=(type=char(1) updatewhereclause=yes name=tipo_cicli dbname="barcode.tipo_cicli" dbalias=".tipo_cicli" )
 column=(type=long updatewhereclause=yes name=fila_1 dbname="fila_1" )
 column=(type=long updatewhereclause=yes name=fila_2 dbname="fila_2" )
 column=(type=long updatewhereclause=yes name=fila_1p dbname="fila_1p" )
 column=(type=long updatewhereclause=yes name=fila_2p dbname="fila_2p" )
 retrieve="select barcode.id_meca,
		 barcode.data_int,   
         barcode.num_int,   
         barcode.tipo_cicli,   
         coalesce(barcode.fila_1, 0) fila_1,   
         coalesce(barcode.fila_2, 0) fila_2,   
         coalesce(barcode.fila_1p, 0) fila_1p,   
         coalesce(barcode.fila_2p, 0) fila_2p   
    FROM barcode 
   WHERE barcode.id_meca = :k_id_meca 
		  and ((barcode.pl_barcode = 0 or barcode.pl_barcode is null) 
			 or (:k_pl_barcode > 0 
			     and barcode.pl_barcode = :k_pl_barcode)) 
		  and (barcode.barcode_lav is null or barcode.barcode_lav = '')
		  and barcode.data_stampa > '01.01.1990'
		  and (barcode.data_sosp < '01.01.1991' or barcode.data_sosp is null)  " arguments=(("k_pl_barcode", number),("k_id_meca", number)) )

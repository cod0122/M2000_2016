 column=(type=datetime updatewhereclause=yes name=data_lav_fin dbname="data_lav_fin" )
 column=(type=date updatewhereclause=yes name=barcode_data_lav_ok dbname="barcode.data_lav_ok" dbalias=".data_lav_ok" )
 column=(type=char(1) updatewhereclause=yes name=causale dbname="barcode.causale" dbalias=".causale" )
 column=(type=char(512) updatewhereclause=yes name=note_lav_fin dbname="note_lav_fin" )
 column=(type=char(1) update=yes updatewhereclause=yes name=flg_dosimetro dbname="flg_dosimetro" values="	1/	 /	3" )
 column=(type=char(1) updatewhereclause=yes name=barcode_flg_dosimetro dbname="barcode.flg_dosimetro" dbalias=".flg_dosimetro" )
 column=(type=char(13) updatewhereclause=yes name=barcode_lav dbname="barcode_lav" )
 column=(type=datetime updatewhereclause=yes name=modgiri_data dbname="barcode.modgiri_data" dbalias=".modgiri_data" )
 column=(type=char(12) updatewhereclause=yes name=modgiri_utente dbname="barcode.modgiri_utente" dbalias=".modgiri_utente" )
 column=(type=long updatewhereclause=yes name=barcode_num_int dbname="barcode.num_int" dbalias=".num_int" )
 column=(type=date updatewhereclause=yes name=barcode_data_int dbname="barcode.data_int" dbalias=".data_int" )
 column=(type=long updatewhereclause=yes name=id_meca dbname="barcode.id_meca" dbalias=".id_meca" )
 column=(type=long updatewhereclause=yes name=id_armo dbname="barcode.id_armo" dbalias=".id_armo" )
 column=(type=datetime updatewhereclause=yes name=x_datins dbname="barcode.x_datins" dbalias=".x_datins" )
 column=(type=char(12) updatewhereclause=yes name=x_utente dbname="barcode.x_utente" dbalias=".x_utente" )
 column=(type=long updatewhereclause=yes name=imptime_second dbname="imptime_second" )
 column=(type=long updatewhereclause=yes name=id_meca_padre dbname="id_meca_padre" )
 column=(type=long updatewhereclause=yes name=k_n_asdrackbarcode dbname="k_n_asdrackbarcode" values="	1/	0" )
 retrieve="  SELECT case 
            when trim(barcode.barcode_lav) > ' ' then trim(barcode.barcode)
            else ''
         end 'barcode',
         concat(barcode.fila_1, '+', barcode.fila_1p) as barcode_fila_1,   
         concat(barcode.fila_2, '+', barcode.fila_2p) as barcode_fila_2,   
         concat(barcode.lav_fila_1, '+', barcode.lav_fila_1p) as barcode_lav_fila_1,   
         concat(barcode.lav_fila_2, '+', barcode.lav_fila_2p) as barcode_lav_fila_2,   
         armo.dose,   
         (armo.pedane/armo.colli_2) occupazione_pedana,   
         armo.campione,   
         armo.art,   
         barcode.pl_barcode,   
         cast(cast(barcode.data_lav_ini as date) as datetime) + cast(barcode.ora_lav_ini as datetime) as barcode_data_lav_ini,   
         cast(cast(barcode.data_lav_fin as date) as datetime) + cast(barcode.ora_lav_fin as datetime) as data_lav_fin,   
         barcode.data_lav_ok,
         barcode.causale,
         trim(barcode.note_lav_fin) as note_lav_fin
  		  , case when barcode.barcode_lav > ' ' then 
              (select case when b1.flg_dosimetro = '1' and b2.flg_dosimetro = '1' then '3' 
                           when b1.flg_dosimetro = '1' then '1'
                           when b2.flg_dosimetro = '1' then '2' 
                           else '' end 
                 from barcode b1 inner join barcode b2 on b1.barcode = b2.barcode_lav
                 where b2.barcode = barcode.barcode)
         else (select case when b1.flg_dosimetro = '1' then '1' else '' end from barcode b1 where b1.barcode = barcode.barcode) 
          end 'flg_dosimetro'
         ,barcode.flg_dosimetro
         ,case 
            when trim(barcode.barcode_lav) > ' ' then trim(barcode.barcode_lav)
            else trim(barcode.barcode)
         end  'barcode_lav'
         ,barcode.modgiri_data
         ,barcode.modgiri_utente
         ,barcode.num_int
         ,barcode.data_int
		 ,barcode.id_meca
		 ,barcode.id_armo
		 ,barcode.x_datins
		 ,barcode.x_utente
       ,COALESCE(barcode.imptime_second,0) imptime_second
       ,case 
            when trim(barcode.barcode_lav) > ' ' then 
						(select b2.id_meca from barcode b2 where b2.barcode = barcode.barcode_lav)
            else barcode.id_meca
         end  'id_meca_padre'
         ,(select case when count(*) > 0 then 1 else 0 end from v_asd_barcode_all where v_asd_barcode_all.barcode = barcode.barcode) k_n_asdrackbarcode
    FROM barcode
                  inner JOIN armo ON 
         barcode.id_armo = armo.id_armo
   WHERE (barcode.id_meca = :k_id) 
  AND NOT EXISTS 
             (SELECT BARCODE 
                    FROM BARCODE B
                     WHERE B.BARCODE_LAV = BARCODE.BARCODE
							 and b.id_meca = :k_id)
order by barcode_lav, barcode 


" arguments=(("k_id", number))  sort="barcode_lav A barcode A " )

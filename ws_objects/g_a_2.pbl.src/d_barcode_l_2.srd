 column=(type=char(13) updatewhereclause=yes name=barcode_lav_fila_2 dbname="barcode_lav_fila_2" )
 column=(type=decimal(1) updatewhereclause=yes name=armo_dose dbname="armo.dose" dbalias=".dose" )
 column=(type=decimal(8) updatewhereclause=yes name=occupazione_pedana dbname="occupazione_pedana" )
 column=(type=char(1) updatewhereclause=yes name=armo_campione dbname="armo.campione" dbalias=".campione" values="	S/	N" )

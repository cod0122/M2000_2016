table(column=(type=long updatewhereclause=yes name=work_table_bilancella dbname="work_table.Bilancella" )
 column=(type=char(12) updatewhereclause=yes name=barcode dbname="work_table.Barcode" )
 column=(type=char(45) updatewhereclause=yes name=info_pallet_cliente dbname="info_pallet.Cliente" )
 column=(type=char(45) updatewhereclause=yes name=info_pallet_lotto dbname="info_pallet.Lotto" )
 column=(type=char(12) updatewhereclause=yes name=groupage_pallet_figlio dbname="groupage.Pallet_Figlio" )
 column=(type=char(45) updatewhereclause=yes name=info_pallet_cliente_figlio dbname="info_pallet.Cliente_Figlio" )
 column=(type=char(45) updatewhereclause=yes name=info_pallet_lotto_figlio dbname="info_pallet.Lotto_Figlio" )
 column=(type=datetime updatewhereclause=yes name=work_table_data_ingresso dbname="work_table.Data_Ingresso" )
 column=(type=number updatewhereclause=yes name=giri_f1_previsti dbname="giri_f1_previsti" )
 column=(type=number updatewhereclause=yes name=giri_f2_previsti dbname="giri_f2_previsti" )
 column=(type=number updatewhereclause=yes name=step_previsti dbname="step_previsti" )
 column=(type=decimal(0) updatewhereclause=yes name=giri_f1_fatti dbname="giri_f1_fatti" )
 column=(type=decimal(0) updatewhereclause=yes name=giri_f2_fatti dbname="giri_f2_fatti" )
 column=(type=decimal(0) updatewhereclause=yes name=step_f1_fatti dbname="step_f1_fatti" )
 column=(type=decimal(0) updatewhereclause=yes name=step_f2_fatti dbname="step_f2_fatti" )
 column=(type=char(3) updatewhereclause=yes name=non_permutata dbname="non_permutata" )
 column=(type=number updatewhereclause=yes name=giri_f1_residui dbname="giri_f1_residui" )
 column=(type=number updatewhereclause=yes name=giri_f2_residui dbname="giri_f2_residui" )
 column=(type=number updatewhereclause=yes name=passi_residui dbname="passi_residui" )
 column=(type=number updatewhereclause=yes name=previsione dbname="previsione" )
 column=(type=datetime updatewhereclause=yes name=prevision_date dbname="prevision_date" )
 retrieve="SELECT 
 w.Bilancella,
w.Barcode,
 i.Cliente,
 i.Lotto,
 g.Pallet_Figlio,
 i2.Cliente Cliente_Figlio,
 i2.Lotto Lotto_Figlio,
 w.Data_Ingresso,  
 w.CicliFila1+w.CicliFila1P giri_f1_previsti,
 w.CicliFila2+w.CicliFila2P giri_f2_previsti,
 8*( (w.CicliFila1+w.CicliFila1P)+2*(w.CicliFila2+w.CicliFila2P)) step_previsti, // giri_f1_previsti * 8 + giri_f2_previsti * 8 * 2
 f1avp+f1app giri_f1_fatti,
 f2avp+f2app giri_f2_fatti,
 (pc.p0+pc.p1+pc.p2+pc.p3+pc.p4+pc.p5+pc.p6+pc.p7) step_f1_fatti,  // step fatti nell'ultimo ciclo
 (pc.p8+pc.p9+pc.p10+pc.p11+pc.p12+pc.p13+pc.p14+pc.p15) step_f2_fatti,
 --if(f1app+f2app+f1avp+f2avp+(pc.p0+pc.p1+pc.p2+pc.p3+pc.p4+pc.p5+pc.p6+pc.p7)+(pc.p8+pc.p9+pc.p10+pc.p11+pc.p12+pc.p13+pc.p14+pc.p15)=0,'>>','') in_ingresso,   -- bilancella nella coda di ingresso (da valutare tempo di ingresso)
  case
         when (f1app+f2app+f1avp+f2avp+(pc.p0+pc.p1+pc.p2+pc.p3+pc.p4+pc.p5+pc.p6+pc.p7)+(pc.p8+pc.p9+pc.p10+pc.p11+pc.p12+pc.p13+pc.p14+pc.p15))=0 then '[+]'
          when f1app+f2app=0 then '<>'
         when ( (w.CicliFila1+w.CicliFila1P-f1avp-f1app)*8 -(pc.p0+pc.p1+pc.p2+pc.p3+pc.p4+pc.p5+pc.p6+pc.p7) + 8*(w.CicliFila2+w.CicliFila2P-f2avp-f2app)*2  -(pc.p8+pc.p9+pc.p10+pc.p11+pc.p12+pc.p13+pc.p14+pc.p15)*2
 + if (f1app+f2app=0,1,0) *8*6 + if(f1app+f2app+f1avp+f2avp=0,1,0) *8*3) = 0 then '-->'
      else ''
  end non_permutata,             -- bilancella non ancora permutata (d valutare tempo di permuta)
-- if(f1app+f2app=0,'<>','') non_permutata,             -- bilancella non ancora permutata (d valutare tempo di permuta)
 w.CicliFila1+w.CicliFila1P-f1avp-f1app giri_f1_residui,	
 w.CicliFila2+w.CicliFila2P-f2avp-f2app  giri_f2_residui,
 (w.CicliFila1+w.CicliFila1P-f1avp-f1app)*8 -(pc.p0+pc.p1+pc.p2+pc.p3+pc.p4+pc.p5+pc.p6+pc.p7) + 8*(w.CicliFila2+w.CicliFila2P-f2avp-f2app)*2  -(pc.p8+pc.p9+pc.p10+pc.p11+pc.p12+pc.p13+pc.p14+pc.p15)*2  passi_residui, -- = (giri_previsti -  giri_fatti) * 8 oppure 16 - step_fatti  * 1 oppure 2
 (w.CicliFila1+w.CicliFila1P-f1avp-f1app)*8 -(pc.p0+pc.p1+pc.p2+pc.p3+pc.p4+pc.p5+pc.p6+pc.p7) + 8*(w.CicliFila2+w.CicliFila2P-f2avp-f2app)*2  -(pc.p8+pc.p9+pc.p10+pc.p11+pc.p12+pc.p13+pc.p14+pc.p15)*2
 + if(f1app+f2app=0,1,0) *8*6 + if(f1app+f2app+f1avp+f2avp=0,1,0) *8*3 previsione  -- ai passi residui aggiunge 6 giri se da permutare + 3 se è in ingresso
, date_add(convert_tz(UTC_TIMESTAMP(), '+0:00', '+1:00')
     , INTERVAL (( ( (w.CicliFila1+w.CicliFila1P-f1avp-f1app)*8 -(pc.p0+pc.p1+pc.p2+pc.p3+pc.p4+pc.p5+pc.p6+pc.p7) + 8
                        *(w.CicliFila2+w.CicliFila2P-f2avp-f2app)*2  -(pc.p8+pc.p9+pc.p10+pc.p11+pc.p12+pc.p13+pc.p14+pc.p15)*2
                      + if(f1app+f2app=0,1,0) *8*6 + if(f1app+f2app+f1avp+f2avp=0,1,0) *8*3) +1) * 250 ) second) prevision_date
 FROM gam1.work_table w
   join gam1.passaggi pc on w.bilancella=pc.bilancella
   join gam1.wip wip on w.Barcode=wip.Pallet_Code and PosBil=1
   left join gam1.groupage g on w.Barcode = g.Padre
   left join gam1.info_pallet i on w.Barcode=i.Barcode
   left join gam1.info_pallet i2 on g.Pallet_Figlio=i2.Barcode 
 where w.NN in (1,2,3,4) 
         and w.Data_Ingresso > 0
 order by previsione, (w.CicliFila1+w.CicliFila1P-f1avp-f1app+w.CicliFila2+w.CicliFila2P-f2avp-f2app), bilancella

" )

 column=(type=char(12) updatewhereclause=yes name=barcode dbname="queue_table.Barcode" dbalias=".Barcode" )
 column=(type=char(2) updatewhereclause=yes name=id_modo dbname="id_modo" )
 column=(type=char(16) updatewhereclause=yes name=ciclo dbname="ciclo" )
 column=(type=decimal(0) updatewhereclause=yes name=giri dbname="giri" )
 column=(type=char(45) updatewhereclause=yes name=lotto dbname="lotto" )
 column=(type=long updatewhereclause=yes name=e1doco dbname="queue_table.e1doco" dbalias=".e1doco" )
 column=(type=char(1) updatewhereclause=yes name=e1ancodrs dbname="e1ancodrs" )
 column=(type=decimal(0) updatewhereclause=yes name=id_meca dbname="id_meca" )
 retrieve="SELECT  queue_table.Ordine 
          , queue_table.Barcode 
          , trim(queue_table.id_modo)  id_modo
          , trim(queue_table.ciclo)  ciclo
          , coalesce(queue_table.giri, 0) giri
          , trim(wo.lotto) lotto
   		 , queue_table.Work_Order as e1doco
          , ' ' e1ancodrs
   		 , 0 id_meca
      FROM queue_table inner join Work_Orders wo on queue_table.Work_Order = wo.Work_Order
		where queue_table.ordine > 
         coalesce((select cast(impostazioni.valore as decimal)
              from impostazioni
              where impostazioni.variabile = 'num_intouchable'), 0)
    order by queue_table.ordine asc     					  
" )

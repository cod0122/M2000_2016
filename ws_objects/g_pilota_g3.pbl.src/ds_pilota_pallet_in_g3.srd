 retrieve="SELECT CONVERT_TZ(pallet.Data_Entrata,@@global.time_zone,'+00:00') as Data_Entrata,   
         CONVERT_TZ(pallet.Data_Uscita,@@global.time_zone,'+00:00') as Data_Uscita,    
         pallet.barcode as barcode,   
         pallet.id_modo as id_modo,   
         pallet.ciclo as ciclo,   
         pallet.giri as giri,   
         pallet.carrier as carrier
    FROM pallet  
   WHERE pallet.Data_Entrata > :k_data_da 
-- and (pallet.Data_Uscita < '01.01.2000 00:00:00' or pallet.Data_Uscita is null)   
	order by pallet.Data_Entrata asc
	       , pallet.barcode 

 retrieve=" SELECT depositi.id_deposito as id_deposito,  
         isnull(depositi.deleted,'') deleted,
         isnull(depositi.operativo,'') operativo,
         trim(depositi.descr) as descr,  
         COALESCE(depositi.id_deposito_anag, 0) as id_deposito_anag
        ,COALESCE((select trim(deposito_anag.rag_soc_1) from deposito_anag where depositi.id_deposito_anag = deposito_anag.id_deposito_anag),'') as deposito_anag_rag_soc_1
        ,COALESCE(trim(depositi.ddt_n_prefisso),'') as ddt_n_prefisso
        ,COALESCE(depositi.ddt_n_ult, 0) as ddt_n_ult
        ,depositi.ddt_data_ult as ddt_data_ult
        ,depositi.x_datins
    FROM depositi   
  WHERE :k_id_deposito = 0 or depositi.id_deposito = :k_id_deposito
  ORDER BY depositi.descr ASC
" arguments=(("k_id_deposito", number)) )

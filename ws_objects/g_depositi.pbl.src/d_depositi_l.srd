 column=(type=char(80) update=yes updatewhereclause=yes name=descr dbname="descr" )
 column=(type=long updatewhereclause=yes name=id_deposito_anag dbname="id_deposito_anag" )
 column=(type=char(60) updatewhereclause=yes name=deposito_anag_rag_soc_1 dbname="deposito_anag_rag_soc_1" )
 column=(type=char(2) updatewhereclause=yes name=ddt_n_prefisso dbname="ddt_n_prefisso" )
 column=(type=long updatewhereclause=yes name=ddt_n_ult dbname="ddt_n_ult" )
 column=(type=date updatewhereclause=yes name=ddt_data_ult dbname="depositi.ddt_data_ult" )

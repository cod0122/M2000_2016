table(column=(type=long updatewhereclause=yes name=id_asddevice dbname="asddevice.id_asddevice" )
 column=(type=char(160) updatewhereclause=yes name=asddevice_des dbname="asddevice_des" )
 column=(type=char(12) updatewhereclause=yes name=cod_sl_pt dbname="cod_sl_pt" )
 column=(type=char(20) updatewhereclause=yes name=densita dbname="densita" )
 column=(type=char(50) updatewhereclause=yes name=descr dbname="descr" )
 column=(type=long updatewhereclause=yes name=id_cliente dbname="id_cliente" )
 column=(type=char(30) updatewhereclause=yes name=rag_soc_10 dbname="rag_soc_10" )
 column=(type=long updatewhereclause=yes name=mis_x dbname="mis_x" )
 column=(type=long updatewhereclause=yes name=mis_y dbname="mis_y" )
 column=(type=long updatewhereclause=yes name=mis_z dbname="mis_z" )
 column=(type=long updatewhereclause=yes key=yes identity=yes name=id_asdslpt dbname="id_asdslpt" )
 column=(type=long update=yes updatewhereclause=yes name=k_associato dbname="k_associato" values="	1/	0" )
 retrieve="  SELECT distinct asddevice.id_asddevice,   
         COALESCE(asddevice.des, '') asddevice_des,   
         COALESCE(sl_pt.cod_sl_pt, '') cod_sl_pt,   
         COALESCE(sl_pt.densita, '') densita,   
         COALESCE(sl_pt.descr, '') descr,   
         COALESCE(sl_pt.id_cliente, 0) id_cliente,   
         COALESCE(clienti.rag_soc_10, '') rag_soc_10,   
         COALESCE(sl_pt.mis_x, 0) mis_x,   
         COALESCE(sl_pt.mis_y, 0) mis_y,   
         COALESCE(sl_pt.mis_z, 0) mis_z,   
         COALESCE(asdslpt.id_asdslpt, 0) id_asdslpt  
         ,1 k_associato
    FROM asdslpt inner JOIN asddevice ON asdslpt.id_asddevice = asddevice.id_asddevice
                 inner join sl_pt on asdslpt.cod_sl_pt = sl_pt.cod_sl_pt
                 LEFT OUTER JOIN clienti ON sl_pt.id_cliente = clienti.codice
   WHERE asddevice.id_asddevice = :k_id_asddevice 
union all 
  SELECT distinct :k_id_asddevice,   
         '' asddevice_des,   
         COALESCE(sl_pt.cod_sl_pt, '') cod_sl_pt,   
         COALESCE(sl_pt.densita, '') densita,   
         COALESCE(sl_pt.descr, '') descr,   
         COALESCE(sl_pt.id_cliente, 0) id_cliente,   
         COALESCE(clienti.rag_soc_10, '') rag_soc_10,   
         COALESCE(sl_pt.mis_x, 0) mis_x,   
         COALESCE(sl_pt.mis_y, 0) mis_y,   
         COALESCE(sl_pt.mis_z, 0) mis_z,   
         0 id_asdslpt  
         , 0 k_associato
    FROM sl_pt
                 LEFT OUTER JOIN clienti ON sl_pt.id_cliente = clienti.codice
                 inner join contratti on sl_pt.cod_sl_pt = contratti.sl_pt
                 inner join listino on contratti.codice = listino.contratto
   WHERE :k_show_all = 'S'
         and contratti.data_scad >= getdate()
         and listino.attivo = 'S'
		   and (getdate() <= listino.dt_end or listino.dt_end < '01.01.1901' or listino.dt_end is null)
    and sl_pt.cod_sl_pt not in (
            select asdslpt.cod_sl_pt 
                   from asdslpt 
                   where asdslpt.id_asddevice = :k_id_asddevice )
" update="associato" updatewhere=0 updatekeyinplace=no arguments=(("k_id_asddevice", number),("k_show_all", string))  sort="k_associato D cod_sl_pt A " )

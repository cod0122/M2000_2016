 retrieve="  SELECT programmi_dettaglio_g2.id_programma,   
         programmi_dettaglio_g2.sequenza,   
         programmi_dettaglio_g2.id_impianto,   
         programmi_dettaglio_g2.barcode,   
         programmi_dettaglio_g2.posizione_bilancella,   
         programmi_dettaglio_g2.giri_f1,   
         programmi_dettaglio_g2.giri_f2,   
         programmi_dettaglio_g2.giri_f1p,   
         programmi_dettaglio_g2.giri_f2p,   
         programmi_dettaglio_g2.codice_cliente,
         programmi_dettaglio_g2.nome_cliente, 
         programmi_dettaglio_g2.riferimento,  
         programmi_dettaglio_g2.locazione
       ,isnull(wo.work_order,0) work_order
    FROM programmi_dettaglio_g2 LEFT OUTER JOIN programmi_work_orders wo ON 
					programmi_dettaglio_g2.id_programma = wo.id_programma and programmi_dettaglio_g2.barcode = wo.barcode  
   WHERE (:k_id_programma = 0 or programmi_dettaglio_g2.id_programma = :k_id_programma)
ORDER BY programmi_dettaglio_g2.id_programma ASC,   
         programmi_dettaglio_g2.sequenza ASC   
" arguments=(("k_id_programma", number)) )

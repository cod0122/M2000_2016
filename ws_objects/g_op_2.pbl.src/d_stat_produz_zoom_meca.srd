table(column=(type=long updatewhereclause=yes name=id_meca dbname="s_armo.id_meca" dbalias=".id_meca" )
 column=(type=long updatewhereclause=yes name=num_int dbname="s_armo.num_int" dbalias=".num_int" )
 column=(type=date updatewhereclause=yes name=data_ent dbname="s_armo.data_ent" dbalias=".data_ent" )
 column=(type=long updatewhereclause=yes name=colli_entrati dbname="colli_entrati" )
 column=(type=long updatewhereclause=yes name=colli_fatturati dbname="colli_fatturati" )
 column=(type=char(43) updatewhereclause=yes name=mandante dbname="mandante" )
 column=(type=char(43) updatewhereclause=yes name=ricevente dbname="ricevente" )
 column=(type=char(43) updatewhereclause=yes name=fatturato dbname="fatturato" )
 column=(type=long updatewhereclause=yes name=colli_trattati dbname="colli_trattati" )
 column=(type=long updatewhereclause=yes name=gruppo dbname="vx_info_stat_guida.gruppo" dbalias="r.gruppo" )
 column=(type=long updatewhereclause=yes name=clie_3 dbname="vx_info_stat_guida.clie_3" dbalias="r.clie_3" )
 column=(type=decimal(2) updatewhereclause=yes name=imp_x_collo dbname="imp_x_collo" )
 column=(type=long updatewhereclause=yes name=id_listino dbname="armo.id_listino" dbalias=".id_listino" )
 column=(type=long updatewhereclause=yes name=impianto dbname="vx_info_stat_guida.impianto" dbalias="r.impianto" )
 column=(type=date updatewhereclause=yes name=data_lav_fin dbname="vx_info_stat_dati.data_lav_fin" dbalias="dati.data_lav_fin" )
 retrieve=" SELECT distinct  
	s_armo.id_meca as id_meca,
	 s_armo.num_int as num_int,   
	 s_armo.data_ent as data_ent,   
	 (dati.colli_entrati) as colli_entrati,
	 0 as colli_fatturati,
	concat(s_armo.clie_1, ' ', clienti_a.rag_soc_10) as mandante,
	concat(s_armo.clie_2, ' ', clienti_b.rag_soc_10) as ricevente,
	concat(s_armo.clie_3 , ' ', clienti_c.rag_soc_10) as fatturato
	,(dati.colli_trattati) as colli_trattati
	,r.gruppo
	,r.clie_3
	,(dati.imp_x_collo) as imp_x_collo
	,armo.id_listino
	,r.impianto impianto
	,dati.data_lav_fin
 from  vx_info_stat_guida as r inner join vx_info_stat_dati as dati  ON 
          r.id_armo = dati.id_armo  
 			  INNER JOIN s_armo on
          r.id_armo = s_armo.id_armo  
 			  INNER JOIN armo on
          r.id_armo = armo.id_armo  
 			  LEFT OUTER JOIN clienti as clienti_a ON 
          s_armo.clie_1 = clienti_a.codice   
                 LEFT OUTER JOIN clienti as clienti_b ON 
          s_armo.clie_2 = clienti_b.codice   
                 LEFT OUTER JOIN clienti as clienti_c ON 
          s_armo.clie_3 = clienti_c.codice   
	group by 
		 s_armo.id_meca ,
		 s_armo.num_int ,   
		 s_armo.data_ent,   
		concat(s_armo.clie_1, ' ', clienti_a.rag_soc_10),
		concat(s_armo.clie_2, ' ', clienti_b.rag_soc_10),
		concat(s_armo.clie_3 , ' ', clienti_c.rag_soc_10)
		,r.gruppo
		,r.clie_3
		,armo.id_listino
		,dati.data_lav_fin
		,r.impianto
,dati.colli_entrati
,dati.colli_trattati
,dati.imp_x_collo

 
" )

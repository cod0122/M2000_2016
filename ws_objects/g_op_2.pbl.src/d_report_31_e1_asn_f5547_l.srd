 retrieve="SELECT  
    CASE WHEN REGEXP_LIKE(trim(F5547013.EHAPID), '^[0-9]+$') THEN TO_NUMBER(trim(F5547013.EHAPID))
       ELSE 0 END AS id_meca,       
    F5547013.EHAN8,      
    F5547013.EHSHAN,      
    F5547013.EHVR01,       
    F5547013.EHA801,        
    F5547014.EDLNID,        
    F5547014.EDLITM,         
    F5547014.EDIR02,         
    F5547014.EDUORG,         
    F5547014.EDUSER,        
    F5547013.EHEDSP,      
    F5547013.EHDS01,   
    F5547014.EDIR01,
    F5547013.EHUPMJ,    
    F5547013.EHTDAY  
FROM  
    F5547013,   
    F5547014   
WHERE  
    (F5547013.EHAPID = F5547014.EDAPID)  
    AND (:k_apid = '0' or F5547013.EHAPID = :k_apid)  
    AND (:k_e1an = 0 or F5547013.EHAN8 = :k_e1an ) 
ORDER BY  
    F5547013.EHAPID desc, F5547014.EDLNID ASC" arguments=(("k_apid", string),("k_e1an", number)) )

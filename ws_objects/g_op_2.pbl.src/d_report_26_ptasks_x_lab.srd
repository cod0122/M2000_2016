 retrieve="  SELECT case 
            when trim(v_ptasks_rows.validation_modaccomprogr)  > ' ' then
                  trim(v_ptasks_rows.validation_modaccomprogr)
            else '-'
            end  validation_modaccomprogr ,   
         coalesce(v_ptasks_rows.validation_modaccompn, 0) validation_modaccompn,  
			v_ptasks_rows.validation_modaccompdata validation_modaccompdata, 
         trim(clienti.rag_soc_10) rag_soc_10,   
         trim(v_ptasks_rows.cs_clienteddtn) cs_clienteddtn,   
         trim(v_ptasks_rows.validation_laboratorio) validation_laboratorio,   
         trim(ptasks_types.descr) descr,   
         trim(v_ptasks_rows.validation_attivitacod) validation_attivitacod,   
         v_ptasks_rows.validation_speddata,   
         v_ptasks_rows.validation_finepresuntadata,   
         case 
            when trim(v_ptasks_rows.validation_laboratoriocertif1n) > ' ' then
					coalesce(trim(v_ptasks_rows.validation_laboratoriocertifn), '') 
                 + ' +' + trim(v_ptasks_rows.validation_laboratoriocertif1n)
	         else coalesce(trim(v_ptasks_rows.validation_laboratoriocertifn), '') 
         end validation_laboratoriocertifn,   
			v_ptasks_rows.validation_laboratoriocertifdata,
         trim(v_ptasks_rows.validation_notealtre) validation_notealtre,
         trim(v_ptasks_rows.validation_offertarif) validation_offertarif, 
         ptasks.n_ptask n_ptask,
         ptasks.date_ins date_ins,
 		   coalesce(validation_campioniqta, 0) validation_campioniqta
        ,ptasks.id_ptask
        ,case 
            when v_ptasks_rows.validation_laboratoriocertif1data > '01.01.1900' then
					FORMAT(v_ptasks_rows.validation_laboratoriocertifdata, 'dd-MMM-yyyy')
                 + ' +' + FORMAT(v_ptasks_rows.validation_laboratoriocertif1data, 'dd-MMM-yyyy')
				else FORMAT(v_ptasks_rows.validation_laboratoriocertifdata, 'dd-MMM-yyyy')
         end validation_laboratoriocertifdata_x 
   ,trim(cast( n_ptask as varchar(10) ) + ' / ' + FORMAT(date_ins, 'yy')) n_ptask_x
    FROM v_ptasks_rows 
                       inner join ptasks on v_ptasks_rows.id_ptask = ptasks.id_ptask 
                       inner join clienti on ptasks.id_cliente = clienti.codice 
                       inner join ptasks_types on v_ptasks_rows.id_ptasks_type = ptasks_types.id_ptasks_type
   WHERE ( :k_id_cliente = 0 OR ptasks.id_cliente = :k_id_cliente ) 
         and ( :k_lab = '' OR v_ptasks_rows.validation_laboratorio = :k_lab ) 
         and ((:k_tipo_data = 0 and v_ptasks_rows.validation_modaccompdata between :k_data_da and :k_data_a ) 
               or (:k_tipo_data = 1 and v_ptasks_rows.validation_speddata between :k_data_da and :k_data_a ) 
               or (:k_tipo_data = 2 and v_ptasks_rows.validation_finepresuntadata between :k_data_da and :k_data_a ) 
               or (:k_tipo_data = 3 and v_ptasks_rows.validation_laboratoriocertifdata between :k_data_da and :k_data_a ) 
             )
   order by ptasks.n_ptask desc
" arguments=(("k_id_cliente", number),("k_lab", string),("k_data_da", date),("k_data_a", date),("k_tipo_data", number)) )

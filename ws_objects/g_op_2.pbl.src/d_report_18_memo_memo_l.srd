 column=(type=char(12) update=yes updatewhereclause=yes name=x_utente dbname="x_utente" )
 column=(type=long updatewhereclause=yes name=id_cliente dbname="clienti_memo.id_cliente" )
 column=(type=long updatewhereclause=yes name=id_meca dbname="meca_memo.id_meca" )
 column=(type=long updatewhereclause=yes name=num_int dbname="meca.num_int" )
 column=(type=date updatewhereclause=yes name=data_int dbname="meca.data_int" )
 column=(type=char(61) updatewhereclause=yes name=rag_soc_10 dbname="rag_soc_10" )
 column=(type=char(12) updatewhereclause=yes name=cod_sl_pt dbname="cod_sl_pt" )
 column=(type=char(32768) updatewhereclause=yes name=memo_plain_text dbname="memo_plain_text" )
 retrieve="  SELECT distinct memo.id_memo,   
         memo.dataora_ins,   
         trim(memo.utente_ins) as utente_ins,   
         (memo.dataora_ins) as data_ins,   
         memo.titolo,   
         memo.note,   
         trim(memo.tipo_memo) as tipo_memo,   
         trim(memo.tipo_sv) as tipo_sv,   
         memo.x_datins,   
         trim(memo.x_utente) as x_utente,
		clienti_memo.id_cliente,
		meca_memo.id_meca ,
		meca.num_int,
		meca.data_int,
         COALESCE(clienti.rag_soc_10,'') + ' ' + COALESCE(clienti.rag_soc_11, '') as rag_soc_10 
     ,COALESCE(sl_pt_memo.cod_sl_pt, '') as cod_sl_pt
     ,'' memo_plain_text 
    FROM memo left outer join memo_link on memo.id_memo = memo_link.id_memo  
                        left outer join clienti_memo on memo.id_memo = clienti_memo.id_memo  
					  left outer join clienti on clienti_memo.id_cliente = clienti.codice
                        left outer join meca_memo on memo.id_memo = meca_memo.id_memo  
					  left outer join meca on meca_memo.id_meca = meca.id
                        left outer join sl_pt_memo on memo.id_memo = sl_pt_memo.id_memo  
   WHERE (:k_ricerca = '%' or (:k_ricerca <> '%' and (memo.titolo like :k_ricerca OR memo.note like :k_ricerca 
                 OR memo_link.titolo like :k_ricerca OR memo_link.link like :k_ricerca OR memo_link.nome like :k_ricerca
                 or sl_pt_memo.cod_sl_pt = :k_ricerca_rid))
          )
                   and (:k_tipo_sv = '' or memo.tipo_sv = :k_tipo_sv)
                   and (:k_id_cliente = 0 or clienti_memo.id_cliente = :k_id_cliente or meca.clie_3 = :k_id_cliente)
                   and (:k_utente_ins = '' or memo.utente_ins = :k_utente_ins or memo.x_utente = :k_utente_ins)
				 and memo.x_datins >= :k_x_datins 
                   
ORDER BY memo.dataora_ins DESC, 
         memo.titolo ASC   
" arguments=(("k_ricerca", string),("k_tipo_sv", string),("k_id_cliente", number),("k_utente_ins", string),("k_x_datins", datetime),("k_ricerca_rid", string)) )

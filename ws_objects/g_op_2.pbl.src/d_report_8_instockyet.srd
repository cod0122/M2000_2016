 column=(type=long updatewhereclause=yes name=e1doco dbname="meca.e1doco" dbalias=".e1doco" )
 column=(type=long updatewhereclause=yes name=e1rorn dbname="meca.e1rorn" dbalias=".e1rorn" )
 column=(type=long updatewhereclause=yes name=colli_scaricati dbname="colli_scaricati" )
 column=(type=datetime updatewhereclause=yes name=data_ent dbname="meca.data_ent" dbalias=".data_ent" )
 column=(type=long updatewhereclause=yes name=dd_late dbname="dd_late" )
 retrieve=" SELECT  meca.id id_meca,   
			meca.clie_3,   
			trim(clienti.rag_soc_10) as Cliente,   
			meca.clie_2 as clie_2,   
			meca.num_int,   
			meca.data_int,   
			meca.num_bolla_in,   
			coalesce(meca.consegna_data, '01.01.1900') consegna_data,   
			armo.colli_2,   
			coalesce(v_arsp_colli_x_id_armo.colli, 0) colli_sped,   
			certif.num_certif,   
			certif.data data_certif  
		 ,meca.e1doco
		 ,meca.e1rorn
		 ,coalesce(v_armo_out_colli.colli, 0) colli_scaricati   
		 ,meca.data_ent data_ent 
      ,(select (sum(cl.workdayflag) + case when sum(cl.weekendflag) > 2 then sum(cl.weekendflag) - (case when (sum(cl.weekendflag)%2) = 0 then 2 else 1 end)
                                      else 0 end) 
                           from u_calendar_working cl 
                      where cl.cal_date between certif.data and :k_today 
                      ) as dd_late
    FROM meca inner join armo on meca.id = armo.id_meca
	 				left outer join v_arsp_colli_x_id_armo on armo.id_armo = v_arsp_colli_x_id_armo.id_armo 
	 				left outer join v_armo_out_colli on armo.id_armo = v_armo_out_colli.id_armo 
				inner join certif on certif.id_meca = meca.id 
				inner join clienti on clienti.codice = meca.clie_3 
	where  certif.data > '1990-01-01'
	       and armo.magazzino = 2
			 and armo.colli_2 > (coalesce(v_arsp_colli_x_id_armo.colli, 0) + coalesce(v_armo_out_colli.colli, 0))
			 and meca.data_int > dateadd(yy, -2, getdate()) 
          and (:k_clie_3 = 0 or meca.clie_3 = :k_clie_3)
" filter="dd_late > ( k_gg_stock_max + 2)"arguments=(("k_gg_stock_max", number),("k_today", date),("k_clie_3", number))  sort="num_int A " )

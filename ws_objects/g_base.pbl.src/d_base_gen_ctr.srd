 retrieve="  SELECT base.id,   
         base.num_int,   
         base.data_int,   
         base.num_certif,   
         base.num_bolla,   
         base.data_bolla,   
         base.num_bolla_stamp,   
         base.data_bolla_stamp,   
         base.num_fatt,   
         base.data_fatt,   
         base.num_fatt_stamp,   
         base.data_fatt_stamp,   
         base.op_sys,   
         base.num_scarico,   
         base.ult_id_armo,   
         base.barcode_pref,   
         base.barcode_progr,   
         base.barcode_progr_man,   
         base.ult_num_pl_barcode,
         base.barcode_dt_no_lav  
       , trim(base.dosimetro_ult_barcode) dosimetro_ult_barcode
       , base.dosimetro_id_armo   
       , trim(base.dosimetro_barcode_mask)  dosimetro_barcode_mask
       , coalesce(base_vari.ddt_out_n_free, 0) ddt_out_n_free
       , coalesce(base_vari.ddt_out_year_free, year(getdate())) ddt_out_year_free
       , coalesce(base_vari.ptasks_valid_modaccompn, 0) ptasks_valid_modaccompn
       , coalesce(base_vari.ptasks_cs_invoicen, 0) ptasks_cs_invoicen

    FROM base 
					left outer join base_vari on 
			base.id = base_vari.id_base
" update="base" updatewhere=0 updatekeyinplace=no )

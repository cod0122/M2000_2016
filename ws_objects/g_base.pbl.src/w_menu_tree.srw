public subroutine u_resize_1 ();//
int k_x, k_y, k_st_y, k_st_width, k_height, k_st_x, k_cbx_x, k_start_x, k_width_tot
int k_nr_icone, k_nr_icone_rel
int k_appon
boolean k_add_spazio_sep, k_separatore


if cbx_txt.thirdstate then
	k_st_width = pb_1.width * 1.7  // testo esposto sotto
	k_height = (pb_1.height + st_1.height) * 1.1 // testo esposto sotto
	k_width_tot = k_st_width 
	st_1.width = k_st_width
else
	if cbx_txt.checked then // compatta
		k_st_width = pb_1.width * 1.45  // testo non esposto 
		k_height = pb_1.height * 1.1  // testo non esposto 
		k_width_tot = k_st_width 
		st_1.width = 0
	else
		k_st_width = pb_1.width * 2.5 // testo esposto a fianco
		k_height = pb_1.height * 1.1  // testo esposto a fianco
		k_width_tot = k_st_width + pb_1.width
		st_1.width = k_st_width
	end if
end if

// cambiato lo stato...
if ki_cbx_txt_stato_changed then
	ki_cbx_txt_stato_changed = false
	this.width = k_width_tot
	//this.setredraw(true)
end if

r_sep1.width = k_width_tot * 0.8 
r_sep2.width = k_width_tot * 0.8

//if this.width > (k_width_tot * (k_nr_icone + 1)) then 
//	k_x = (this.width - (k_width_tot * (k_nr_icone + 1))) / 2
//	k_cbx_x = k_x
//else
	k_x = (pb_1.width * 1.6 - pb_1.width) / 2 
	k_cbx_x = (pb_1.width * 1.5 - cbx_txt.width / 2) / 2 
//end if
//kpb_width = pb_1.width * 0.15 

k_start_x = k_x

cbx_txt.x = k_cbx_x
cbx_txt.y = (pb_1.height - cbx_txt.height) / 2
st_cbx_txt.x = k_cbx_x + cbx_txt.width + 5
st_cbx_txt.y = cbx_txt.y
st_cbx_txt.visible = (not cbx_txt.checked) 

k_nr_icone = this.width / k_width_tot 
//k_nr_icone --
if k_nr_icone > 1 then 
	k_x = k_width_tot // k_st_width + kpb_width + 100 // con più icone sulla stessa riga sposta l'inizio della prima
	k_separatore=false  // tolgo separatori
	k_nr_icone -= 1  // tolgo i separatori
	k_y = 1
else
//--- + due a spazio 
	k_separatore=true  // add i separtori
	k_nr_icone += 2  // add i separtori
	k_y = pb_1.height
	k_nr_icone = 1 //this.width / k_width_tot 
end if

if cbx_txt.thirdstate then
	k_st_x = 0 //- (k_st_width - pb_1.width) / 2 
	k_st_y = pb_1.height
else
	if cbx_txt.checked then // testo non esposto
		k_st_x = 0
		k_st_y = 0
	else
		k_st_x = - pb_1.width * 1.2
		k_st_y = pb_1.height / 3
	end if
end if

	k_nr_icone_rel = k_nr_icone   

	//--- FUNZIONI TIPO STAMPA 
	if u_resize_set_pb(pb_8, st_8, k_nr_icone, k_nr_icone_rel, k_x, k_y, k_start_x, k_st_width, k_st_y, k_st_x,k_width_tot, k_height) then
		k_add_spazio_sep = true
	end if
	if u_resize_set_pb(pb_9, st_9, k_nr_icone, k_nr_icone_rel, k_x, k_y, k_start_x, k_st_width, k_st_y, k_st_x,k_width_tot, k_height) then
		k_add_spazio_sep = true
	end if
	//--- SALVA/NUOVO/...
	if u_resize_set_pb(pb_10, st_10, k_nr_icone, k_nr_icone_rel, k_x, k_y, k_start_x, k_st_width, k_st_y, k_st_x,k_width_tot, k_height) then
		k_add_spazio_sep = true
	end if
	if u_resize_set_pb(pb_11, st_11, k_nr_icone, k_nr_icone_rel, k_x, k_y, k_start_x, k_st_width, k_st_y, k_st_x,k_width_tot, k_height) then

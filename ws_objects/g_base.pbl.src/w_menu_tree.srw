private function boolean u_resize_set_pb (ref picturebutton apb_1, ref statictext ast_1, ref integer a_nr_icone, ref integer a_nr_icone_rel, ref integer k_x, ref integer k_y, integer k_start_x, integer k_st_width, integer k_st_y, integer k_st_x, integer k_width_all, integer k_height);//
//--- posiziona PB e ST del menu in x e y
//
//
boolean k_spazio_bianco = false


if apb_1.tag = "true" then
	k_spazio_bianco = true
	
	if k_st_y = 0 then
		ast_1.visible = false
	end if
	
	apb_1.x = k_x
	apb_1.y = k_y
	ast_1.width = k_st_width
	ast_1.x = apb_1.x - k_st_x 
//	if ast_1.x < 2 then ast_1.x = 2
	ast_1.y = apb_1.y + k_st_y
	a_nr_icone_rel --
	if a_nr_icone_rel > 0 then
		k_x += k_width_all
	else
		k_x = k_start_x
		k_y += k_height //+ kpb_height
		a_nr_icone_rel = a_nr_icone
	end if
else
	ast_1.visible = false
end if

return k_spazio_bianco


end function

private subroutine u_cbx_txt_checked_save ();string k_rc
st_profilestring_ini kst_profile_string_ini

	
	if cbx_txt.thirdstate then
		kst_profile_string_ini.valore = "2"
	else
		if cbx_txt.checked then
			kst_profile_string_ini.valore = "0"
		else
			kst_profile_string_ini.valore = "1"
		end if
	end if
	kst_profile_string_ini.nome = "menutoolbar_testo"
	k_rc = kGuf_data_base.profilestring_ini(kst_profile_string_ini)


end subroutine

public subroutine u_set_cbx_txt ();/*
Imposta lo stato come indicato in Confdb.ini (l'ultimo settaggio)
*/
string k_rc
st_profilestring_ini kst_profilestring_ini


kst_profilestring_ini.nome = "menutoolbar_testo"
k_rc = kGuf_data_base.profilestring_ini(kst_profilestring_ini)

if kst_profilestring_ini.esito = "0" then
	if kst_profilestring_ini.valore = "0" then
		cbx_txt.checked = true
	else
		if kst_profilestring_ini.valore = "2" then
			cbx_txt.thirdstate = true
		else
			cbx_txt.checked = false

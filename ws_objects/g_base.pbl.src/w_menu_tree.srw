		k_add_spazio_sep = true
	end if
	//NO ALLA DELETE if u_resize_set_pb(pb_15, st_15, k_nr_icone, k_nr_icone_rel, k_x, k_y, k_start_x, k_st_width, k_st_y, k_st_x,k_width_tot, k_height)

	if k_add_spazio_sep and k_separatore then
		u_resize_set_sep(r_sep1, k_x, k_y, k_start_x, k_st_x, k_height)
	else
		r_sep1.visible = false
	end if


	//--- Inizio di TROVA/FILTRO
	if u_resize_set_pb(pb_t1, st_t1, k_nr_icone, k_nr_icone_rel, k_x, k_y, k_start_x, k_st_width, k_st_y, k_st_x,k_width_tot, k_height) then

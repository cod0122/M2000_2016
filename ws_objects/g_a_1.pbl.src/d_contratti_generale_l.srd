table(column=(type=char(1) updatewhereclause=yes name=k_tipo_contratto dbname="k_tipo_contratto" values="R.& S.	0/Capitolato	4/Comm.(old)	5/Quotazione	6/Contratto CO	7/" )
 column=(type=char(12) updatewhereclause=yes name=nr_contratto dbname="nr_contratto" )
 column=(type=long updatewhereclause=yes name=anno dbname="contratti_rd.anno" )
 column=(type=date updatewhereclause=yes name=data_contratto dbname="contratti_rd.data_contratto" )
 column=(type=decimal(0) updatewhereclause=yes name=id_cliente dbname="contratti_rd.id_cliente" )
 column=(type=char(30) updatewhereclause=yes name=rag_soc_10 dbname="clienti.rag_soc_10" )
 column=(type=char(4000) updatewhereclause=yes name=note dbname="contratti_rd.note" )
 column=(type=date updatewhereclause=yes name=data_inizio dbname="contratti_rd.data_inizio" )
 column=(type=date updatewhereclause=yes name=data_fine dbname="contratti_rd.data_fine" )
 column=(type=long updatewhereclause=yes name=id_docprod_file dbname="contratti_rd.id_docprod_file" )
 column=(type=datetime updatewhereclause=yes name=x_datins dbname="contratti_rd.x_datins" )
 column=(type=char(12) updatewhereclause=yes name=x_utente dbname="contratti_rd.x_utente" )
 retrieve="  SELECT 
         '0' as k_tipo_contratto,
         trim('' + CONVERT(varchar(10), contratti_rd.id_contratto_rd)) as nr_contratto,   
         contratti_rd.anno,   
         contratti_rd.offerta_data as data_contratto,   
         contratti_rd.id_cliente,   
         clienti.rag_soc_10,   
         contratti_rd.oggetto as note,   
         contratti_rd.data_inizio,   
         contratti_rd.data_fine,   
		contratti_rd.id_docprod as id_docprod_file,
         contratti_rd.x_datins,   
         contratti_rd.x_utente
    FROM contratti_rd  	LEFT OUTER JOIN clienti ON contratti_rd.id_cliente = clienti.codice 
  WHERE (:k_id_cliente = 0 or contratti_rd.id_cliente = :k_id_cliente)
         	and data_fine >= :k_data_ini   
union all
  SELECT 
         '6' as k_tipo_contratto,
         trim('' + CONVERT(varchar(10), contratti_doc.id_contratto_doc)) as nr_contratto,   
         contratti_doc.anno,   
         contratti_doc.offerta_data as data_contratto,   
         contratti_doc.id_cliente,   
         clienti.rag_soc_10,   
         contratti_doc.oggetto as note,   
         contratti_doc.data_inizio,   
         contratti_doc.data_fine,   
		contratti_doc.id_docprod as id_docprod_file,
         contratti_doc.x_datins,   
         contratti_doc.x_utente
    FROM v_contratti_doc contratti_doc	LEFT OUTER JOIN clienti ON contratti_doc.id_cliente = clienti.codice 
  WHERE (:k_id_cliente = 0 or contratti_doc.id_cliente = :k_id_cliente)
         	and data_fine >= :k_data_ini    
union all
  SELECT 
         '5' as k_tipo_contratto,
         trim('' + CONVERT(varchar(10),contratti_co.id_contratto_co)) as nr_contratto,   
         contratti_co.anno,   
         contratti_co.offerta_data as data_contratto,   
         contratti_co.id_cliente,   
         clienti.rag_soc_10,   
         contratti_co.oggetto as note,   
         contratti_co.data_inizio,   
         contratti_co.data_fine,   
		contratti_co.id_docprod as id_docprod_file,
         contratti_co.x_datins,   
         contratti_co.x_utente  
    FROM contratti_co  	LEFT OUTER JOIN clienti ON contratti_co.id_cliente = clienti.codice 
  WHERE (:k_id_cliente = 0 or contratti_co.id_cliente = :k_id_cliente)
         	and data_fine  >= :k_data_ini    
union all
  SELECT 
         '7' as k_tipo_contratto,
         trim('' + CONVERT(varchar(10),contratti.codice)),
         year(contratti.data),   
         contratti.data,   
         contratti.cod_cli,   
         clienti.rag_soc_10,
         contratti.descr,   
         contratti.data,   
         contratti.data_scad   
		,0 as id_docprod_file
       ,contratti.x_datins
       ,contratti.x_utente
--         COALESCE(year(contratti.data_scad),year(contratti.data)) as anno_contratto
    from contratti left outer join clienti on 
         contratti.cod_cli = clienti.codice 
    where (contratti.cod_cli = :k_id_cliente or :k_id_cliente = 0)
         	and contratti.data_scad  >= :k_data_ini    
union all
  SELECT    
         '4' as k_tipo_contratto,
		trim(sc_cf.codice),
         year(sc_cf.data),   
         sc_cf.data,   
         sc_cf.cod_cli,   
         clienti.rag_soc_10,   
         sc_cf.descr,   
         sc_cf.data,   
         sc_cf.data_scad   
		,0 as id_docprod_file
       ,sc_cf.x_datins
       ,sc_cf.x_utente
    FROM sc_cf  LEFT OUTER JOIN clienti  ON sc_cf.cod_cli = clienti.codice 
   WHERE (sc_cf.cod_cli = :k_id_cliente or :k_id_cliente = 0)   
         	and sc_cf.data_scad >= :k_data_ini   

    order by 6 asc, 3 desc, 4 desc, 1 asc

" arguments=(("k_data_ini", date),("k_id_cliente", number)) )

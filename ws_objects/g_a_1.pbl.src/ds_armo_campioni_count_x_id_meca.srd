table(column=(type=long updatewhereclause=yes name=campionecolli dbname="campionecolli" )
 column=(type=long updatewhereclause=yes name=contati dbname="contati" )
 column=(type=long updatewhereclause=yes name=id_armo dbname="id_armo" )
 retrieve="select sum(t1.campionecolli) campionecolli
       ,sum(t1.contati) contati
       ,t1.id_armo
	   from 
(SELECT coalesce(sum(coalesce(armo.campionecolli, 0)),0) as campionecolli
       , 0 as contati
       , armo.id_armo as id_armo
    FROM armo 
	where armo.id_meca = :k_id_meca and armo.campione = 'S'
   group by armo.id_armo
union all
  SELECT 0 
       , coalesce(count(*), 0) contati
       , armo_campioni.id_armo
    FROM armo_campioni inner join armo on armo_campioni.id_armo = armo.id_armo
	where armo.id_meca = :k_id_meca
   group by armo_campioni.id_armo
 ) t1
 group by t1.id_armo" arguments=(("k_id_meca", number)) )

 retrieve="  SELECT 
         contratti.codice,
         contratti.mc_co,   
         contratti.data_scad,   
         contratti.descr
         ,listino.id
         ,listino.cod_cli
         ,clienti.rag_soc_10
         ,listino.dt_end
    from contratti inner join listino on 
         contratti.codice = listino.contratto 
         and listino.attivo = 'S'
						 inner join clienti on 
         listino.cod_cli = clienti.codice 
    where contratti.codice = :k_codice
--          and  contratti.data_scad >= CONVERT (date, GETDATE()) 
          and listino.dt_end > :k_data_scad
--    order by listino.id

          " arguments=(("k_codice", number),("k_data_scad", date)) )
